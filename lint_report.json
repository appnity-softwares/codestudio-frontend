:"ArrayExpression","endLine":236,"endColumn":36,"suggestions":[{"desc":"Update the dependencies array to be: [activeContact, currentUser, socket]","fix":{"range":[9174,9202],"text":"[activeContact, currentUser, socket]"}}]}],"suppressedMessages":[],"errorCount":3,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"\"use client\";\n\nimport { useState, useEffect, useRef } from \"react\";\nimport {\n    Search, Phone, Video, MoreVertical, Send, MoreHorizontal,\n    Paperclip, Smile, Code, Bot, Check, CheckCheck\n} from \"lucide-react\";\nimport { Input } from \"@/components/ui/input\";\nimport { Button } from \"@/components/ui/button\";\nimport { Avatar, AvatarFallback, AvatarImage } from \"@/components/ui/avatar\";\nimport { ScrollArea } from \"@/components/ui/scroll-area\";\nimport { useAuth } from \"@/context/AuthContext\";\nimport { motion } from \"framer-motion\";\nimport { messagesAPI, uploadAPI } from \"@/lib/api\";\nimport { useNavigate, useParams } from \"react-router-dom\";\nimport { useSocket } from \"@/context/SocketContext\";\nimport { format } from \"date-fns\";\nimport { Skeleton } from \"@/components/ui/skeleton\";\n\n// type Message ...\n\ntype Message = {\n    id: string;\n    senderId: string;\n    text: string;\n    attachmentUrl?: string;\n    createdAt: Date;\n    isRead: boolean;\n};\n\ntype Contact = {\n    id: string;\n    username: string;\n    name: string;\n    image?: string;\n    isOnline?: boolean;\n    lastSeen?: string;\n};\n\nexport default function Chat() {\n    const navigate = useNavigate();\n    const { username: urlUsername } = useParams();\n    const { user: currentUser } = useAuth();\n    const { socket } = useSocket();\n\n    const [contacts, setContacts] = useState<Contact[]>([]);\n    const [activeContact, setActiveContact] = useState<Contact | null>(null);\n    const [messages, setMessages] = useState<Message[]>([]);\n    const [newMessage, setNewMessage] = useState(\"\");\n    const [searchQuery, setSearchQuery] = useState(\"\");\n    const [typingUser, setTypingUser] = useState<string | null>(null);\n    const [isHistoryLoading, setIsHistoryLoading] = useState(false);\n    const [isConnected, setIsConnected] = useState(false);\n\n    const fileInputRef = useRef<HTMLInputElement>(null);\n    const messagesEndRef = useRef<HTMLDivElement>(null);\n\n    const [refreshKey, setRefreshKey] = useState(0);\n\n    // Load Contacts (Active Conversations Only)\n    useEffect(() => {\n        const fetchContacts = async () => {\n            if (!currentUser?.id) return;\n            try {\n                const res = await messagesAPI.getContacts();\n                const contactsList: Contact[] = res.contacts.map((u: any) => ({\n                    id: u.id,\n                    username: u.username,\n                    name: u.name,\n                    image: u.image,\n                    isOnline: false,\n                    lastSeen: 'Offline'\n                }));\n\n                // Handle target user from session storage (e.g., user clicked \"Message\" on profile)\n                const targetUserStr = sessionStorage.getItem('chatTargetUser');\n                if (targetUserStr) {\n                    try {\n                        const targetUser = JSON.parse(targetUserStr);\n                        const exists = contactsList.find(c => c.id === targetUser.id);\n                        if (!exists) {\n                            contactsList.unshift({\n                                id: targetUser.id,\n                                username: targetUser.username,\n                                name: targetUser.name,\n                                image: targetUser.image,\n                                isOnline: false,\n                                lastSeen: 'Recently'\n                            });\n                        }\n                        sessionStorage.removeItem('chatTargetUser');\n                    } catch (e) { console.error(\"Error parsing target user\", e); }\n                }\n\n                setContacts(contactsList);\n\n                // Handle active contact based on URL or session\n                if (urlUsername) {\n                    const contact = contactsList.find(c => c.username === urlUsername);\n                    if (contact) {\n                        setActiveContact(contact);\n                    }\n                } else if (contactsList.length > 0 && !urlUsername) { // Only redirect if no URL param\n                    // Optional: auto-select first? Maybe better not to force navigation if user just wants to see list.\n                    // setActiveContact(contactsList[0]);\n                    // navigate(`/chat/${contactsList[0].username}`, { replace: true });\n                }\n            } catch (err) { console.error(\"Failed to load contacts\", err); }\n        };\n        fetchContacts();\n    }, [currentUser, urlUsername, refreshKey]);\n\n    // Connection Status Listener\n    useEffect(() => {\n        if (!socket) return;\n\n        const onConnect = () => setIsConnected(true);\n        const onDisconnect = () => setIsConnected(false);\n\n        setIsConnected(socket.connected);\n\n        socket.on(\"connect\", onConnect);\n        socket.on(\"disconnect\", onDisconnect);\n\n        socket.on(\"new_message\", (data: any) => {\n            // 1. Update Messages if active\n            if (activeContact?.id === data.senderId || activeContact?.id === data.receiverId) {\n                setMessages(prev => {\n                    if (prev.some(m => m.id === data.id)) return prev;\n                    return [...prev, {\n                        id: data.id || Date.now().toString(),\n                        senderId: data.senderId,\n                        text: data.text,\n                        attachmentUrl: data.attachmentUrl,\n                        createdAt: data.createdAt ? new Date(data.createdAt) : new Date(),\n                        isRead: false\n                    }];\n                });\n            }\n\n            // 2. Update Contacts List (Reorder or Add)\n            let isNewContact = false;\n            setContacts(prev => {\n                const senderId = data.senderId === currentUser?.id ? data.receiverId : data.senderId;\n                const existingIndex = prev.findIndex(c => c.id === senderId);\n\n                if (existingIndex > -1) {\n                    // Move to top\n                    const updated = [...prev];\n                    const [moved] = updated.splice(existingIndex, 1);\n                    return [moved, ...updated];\n                } else {\n                    isNewContact = true;\n                    return prev;\n                }\n            });\n\n            if (isNewContact) {\n                setRefreshKey(prev => prev + 1);\n            }\n        });\n\n        socket.on(\"user_typing\", (data: { chatId: string, userId: string }) => {\n            if (activeContact && data.userId === activeContact.id) {\n                setTypingUser(activeContact.username);\n                setTimeout(() => setTypingUser(null), 3000);\n            }\n        });\n\n        socket.on(\"online_users\", (onlineUserIds: string[]) => {\n            setContacts(prev => prev.map(contact => ({\n                ...contact,\n                isOnline: onlineUserIds.includes(contact.id)\n            })));\n        });\n\n        socket.on(\"presence_update\", (data: { userId: string, isOnline: boolean }) => {\n            setContacts(prev => prev.map(contact =>\n                contact.id === data.userId\n                    ? { ...contact, isOnline: data.isOnline, lastSeen: data.isOnline ? 'Now' : 'Just now' }\n                    : contact\n            ));\n        });\n\n        return () => {\n            socket.off(\"connect\", onConnect);\n            socket.off(\"disconnect\", onDisconnect);\n            socket.off(\"new_message\");\n            socket.off(\"user_typing\");\n            socket.off(\"online_users\");\n            socket.off(\"presence_update\");\n        };\n    }, [socket, activeContact, currentUser]);\n\n    // Fetch History & Join Room when Active Contact Changes\n    useEffect(() => {\n        if (!activeContact || !currentUser) return;\n\n        const fetchHistory = async () => {\n            setIsHistoryLoading(true);\n            try {\n                const res = await messagesAPI.getHistory(activeContact.id);\n                const mapped = res.messages.map((m: any) => ({\n                    id: m.id,\n                    senderId: m.senderId,\n                    text: m.content,\n                    attachmentUrl: m.attachmentUrl,\n                    createdAt: new Date(m.createdAt),\n                    isRead: m.isRead\n                }));\n                setMessages(mapped);\n\n                const chatId = [currentUser.id, activeContact.id].sort().join(\"-\");\n                if (socket) socket.emit(\"join_chat\", chatId);\n\n                // Mark messages from active contact as read\n                await messagesAPI.markAsRead(activeContact.id);\n            } catch (e) { console.error(\"Failed to load history\", e); }\n            finally { setIsHistoryLoading(false); }\n        };\n        fetchHistory();\n\n        // Listen for read receipts\n        socket?.on(\"messages_read\", (data: { readBy: string, senderId: string }) => {\n            if (data.senderId === currentUser.id) {\n                // Our messages were read - update local state\n                setMessages(prev => prev.map(m =>\n                    m.senderId === currentUser.id ? { ...m, isRead: true } : m\n                ));\n            }\n        });\n\n        return () => {\n            socket?.off(\"messages_read\");\n        };\n    }, [activeContact, currentUser]);\n\n\n    const scrollToBottom = () => { messagesEndRef.current?.scrollIntoView({ behavior: \"smooth\" }); };\n    useEffect(scrollToBottom, [messages, activeContact]);\n\n    const handleSendMessage = (attachmentUrl?: string) => {\n        if ((!newMessage.trim() && !attachmentUrl) || !activeContact || !currentUser) return;\n\n        const chatId = [currentUser.id, activeContact.id].sort().join(\"-\");\n        const msgData = {\n            chatId,\n            senderId: currentUser.id,\n            receiverId: activeContact.id,\n            text: newMessage,\n            attachmentUrl\n        };\n\n        socket?.emit(\"send_message\", msgData);\n\n        const localMsg: Message = {\n            id: 'temp-' + Date.now(),\n            senderId: currentUser.id,\n            text: newMessage,\n            attachmentUrl,\n            createdAt: new Date(),\n            isRead: false\n        };\n\n        setMessages(prev => [...prev, localMsg]);\n        setNewMessage(\"\");\n    };\n\n    const handleTyping = (e: React.ChangeEvent<HTMLInputElement>) => {\n        setNewMessage(e.target.value);\n        if (activeContact && currentUser && socket) {\n            const chatId = [currentUser.id, activeContact.id].sort().join(\"-\");\n            socket.emit(\"typing\", { chatId, userId: currentUser.id });\n        }\n    };\n\n    const handleFileUpload = async (e: React.ChangeEvent<HTMLInputElement>) => {\n        const file = e.target.files?.[0];\n        if (!file) return;\n\n        try {\n            const res = await uploadAPI.chatAttachment(file);\n            handleSendMessage(res.url);\n        } catch (err) {\n            console.error(\"Upload failed\", err);\n        }\n    };\n\n    return (\n        <div className=\"h-[calc(100vh-6rem)] grid grid-cols-1 md:grid-cols-[300px_1fr] lg:grid-cols-[350px_1fr] bg-black/20 backdrop-blur-3xl overflow-hidden rounded-[2rem] border border-white/5 m-4\">\n            {/* Sidebar / Contacts List */}\n            <div className=\"border-r border-white/5 bg-white/5 flex flex-col\">\n                <div className=\"p-6 border-b border-white/5 space-y-4\">\n                    <div className=\"flex items-center justify-between\">\n                        <div className=\"flex flex-col\">\n                            <h2 className=\"text-xl font-black font-headline tracking-tighter italic\">Encrypted Comms</h2>\n                            <div className=\"flex items-center gap-1.5 mt-1\">\n                                <div className={`h-1.5 w-1.5 rounded-full ${isConnected ? 'bg-green-500 animate-pulse' : 'bg-red-500'}`} />\n                                <span className=\"text-[10px] uppercase tracking-widest text-muted-foreground font-bold\">\n                                    {isConnected ? 'Secure Link Active' : 'Offline / Reconnecting'}\n                                </span>\n                            </div>\n                        </div>\n                        <Button size=\"icon\" variant=\"ghost\" className=\"rounded-full h-8 w-8 hover:bg-white/10\">\n                            <MoreVertical className=\"h-4 w-4\" />\n                        </Button>\n                    </div>\n                    <div className=\"relative\">\n                        <Search className=\"absolute left-3 top-1/2 -translate-y-1/2 h-4 w-4 text-muted-foreground\" />\n                        <Input\n                            placeholder=\"Search operatives...\"\n                            className=\"bg-black/20 border-white/10 pl-9 rounded-xl focus:ring-primary/50 text-xs\"\n                            value={searchQuery}\n                            onChange={(e) => setSearchQuery(e.target.value)}\n                        />\n                    </div>\n                </div>\n\n                <ScrollArea className=\"flex-1\">\n                    <div className=\"p-3 space-y-2\">\n                        {contacts\n                            .filter(c => c.username.toLowerCase().includes(searchQuery.toLowerCase()))\n                            .map(contact => (\n                                <button\n                                    key={contact.id}\n                                    onClick={() => navigate(`/chat/${contact.username}`)}\n                                    className={`w-full flex items-center gap-3 p-3 rounded-xl transition-all ${activeContact?.id === contact.id ? 'bg-primary/10 border border-primary/20' : 'hover:bg-white/5 border border-transparent'}`}\n                                >\n                                    <div className=\"relative\">\n                                        <Avatar className=\"h-10 w-10 border border-white/10\">\n                                            <AvatarImage src={contact.image} />\n                                            <AvatarFallback className=\"text-[10px] font-bold\">{contact.username[0].toUpperCase()}</AvatarFallback>\n                                        </Avatar>\n                                        {contact.isOnline && (\n                                            <span className=\"absolute bottom-0 right-0 h-2.5 w-2.5 bg-green-500 border-2 border-black rounded-full\" />\n                                        )}\n                                    </div>\n                                    <div className=\"flex-1 text-left overflow-hidden\">\n                                        <div className=\"flex justify-between items-center mb-0.5\">\n                                            <span className={`text-sm font-bold truncate ${activeContact?.id === contact.id ? 'text-primary' : 'text-white'}`}>\n                                                {contact.username}\n                                            </span>\n                                            <span className=\"text-[10px] text-muted-foreground\">{contact.lastSeen}</span>\n                                        </div>\n                                        <p className=\"text-[11px] text-muted-foreground truncate\">\n                                            {activeContact?.id === contact.id ? 'Decrypting message stream...' : 'Click to initialize connection'}\n                                        </p>\n                                    </div>\n                                </button>\n                            ))}\n                    </div>\n                </ScrollArea>\n            </div>\n\n            {/* Chat Area */}\n            {activeContact ? (\n                <div className=\"flex flex-col h-full bg-gradient-to-b from-transparent to-black/20\">\n\n                    {/* Header */}\n                    <div className=\"h-20 border-b border-white/5 px-6 flex items-center justify-between shrink-0 bg-white/5 backdrop-blur-md\">\n                        <div className=\"flex items-center gap-4\">\n                            <Avatar className=\"h-10 w-10 border border-white/10\">\n                                <AvatarImage src={activeContact.image} />\n                                <AvatarFallback>{activeContact.username[0]}</AvatarFallback>\n                            </Avatar>\n                            <div>\n                                <h3 className=\"font-bold text-white flex items-center gap-2\">\n                                    {activeContact.username}\n                                    {activeContact.isOnline && <span className=\"inline-block w-1.5 h-1.5 bg-green-500 rounded-full animate-pulse\" />}\n                                </h3>\n                                <div className=\"h-4\">\n                                    {typingUser ? (\n                                        <p className=\"text-[10px] text-primary animate-pulse font-black uppercase tracking-widest italic\">Operative is typing...</p>\n                                    ) : (\n                                        <p className=\"text-xs text-muted-foreground\">Premium Operative  {activeContact.isOnline ? 'Active' : activeContact.lastSeen}</p>\n                                    )}\n                                </div>\n             