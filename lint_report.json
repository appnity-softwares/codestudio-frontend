t } from \"react\";\nimport { useParams, Link } from \"react-router-dom\";\nimport { useQuery, useMutation, useQueryClient } from \"@tanstack/react-query\";\nimport {\n    Github, MapPin, Calendar, Code, Users, UserCheck,\n    Linkedin, Zap, Sparkles, Settings, LayoutDashboard, Bookmark, Terminal, FileText, Shield\n} from \"lucide-react\";\nimport { Button } from \"@/components/ui/button\";\nimport { Avatar, AvatarFallback, AvatarImage } from \"@/components/ui/avatar\";\nimport { Tabs, TabsContent, TabsList, TabsTrigger } from \"@/components/ui/tabs\";\nimport {\n    Dialog,\n    DialogContent,\n    DialogHeader,\n    DialogTitle,\n    DialogTrigger,\n} from \"@/components/ui/dialog\";\nimport { Textarea } from \"@/components/ui/textarea\";\nimport { usersAPI, snippetsAPI, authAPI } from \"@/lib/api\";\nimport { SnippetCard } from \"@/components/SnippetCard\";\nimport { useAuth } from \"@/context/AuthContext\";\nimport { format } from \"date-fns\";\nimport { motion, AnimatePresence } from \"framer-motion\";\nimport { LatexRenderer } from \"@/components/LatexRenderer\";\n\nimport { ProfileDashboard } from \"@/components/profile/ProfileDashboard\";\nimport { ProfileSaved } from \"@/components/profile/ProfileSaved\";\nimport { ProfileSettings } from \"@/components/profile/ProfileSettings\";\n\nimport { useTheme } from \"@/context/ThemeContext\";\nimport { useToast } from \"@/hooks/use-toast\";\n\nimport SkillRadar from \"@/components/profile/SkillRadar\";\nimport BadgeGrid from \"@/components/profile/BadgeGrid\";\n\nexport default function Profile() {\n    const { username } = useParams<{ username: string }>();\n    const { user: currentUser, signOut, updateUser } = useAuth();\n    const { theme, setTheme } = useTheme();\n    const { toast } = useToast();\n    const [view, setView] = useState(\"snippets\");\n    const [isEditingResume, setIsEditingResume] = useState(false);\n    const [editedResume, setEditedResume] = useState(\"\");\n\n    // Dashboard State\n    const [dashboardSnippets, setDashboardSnippets] = useState<any[]>([]);\n\n    // Saved State\n    const [savedSnippets, setSavedSnippets] = useState<any[]>([]);\n\n    // Settings State\n    const [isLoadingSettings, setIsLoadingSettings] = useState(false);\n    const [name, setName] = useState(\"\");\n    const [editedUsername, setEditedUsername] = useState(\"\");\n    const [bio, setBio] = useState(\"\");\n    const [visibility, setVisibility] = useState(\"PUBLIC\");\n    const [profileImage, setProfileImage] = useState(\"\");\n    const [githubUrl, setGithubUrl] = useState(\"\");\n    const [instagramUrl, setInstagramUrl] = useState(\"\");\n\n    const queryClient = useQueryClient();\n\n    // 1. Fetch User\n    const { data: userResponse, isLoading: userLoading } = useQuery({\n        queryKey: ['user', username],\n        queryFn: () => {\n            if (username === 'me') {\n                return authAPI.me();\n            }\n            return usersAPI.getById(username!);\n        },\n        retry: false\n    });\n\n    const convertToLaTeX = (text: string) => {\n        let latex = text;\n        latex = latex.replace(/^(#\\s*|SECTION:\\s*)(.+)$/gim, '\\\\section*{$2}');\n        latex = latex.replace(/(\\*\\*|__)(.*?)\\1/g, '\\\\textbf{$2}');\n        latex = latex.replace(/(\\*|_)(.*?)\\1/g, '\\\\textit{$2}');\n        if (latex.includes('\\n- ') || latex.includes('\\n* ')) {\n            latex = latex.replace(/^[-*]\\s+(.+)$/gm, '\\\\item $1');\n            if (!latex.includes('\\\\begin{itemize}')) {\n                latex = '\\\\begin{itemize}\\n' + latex + '\\n\\\\end{itemize}';\n            }\n        }\n        latex = latex.replace(/^\\[(.+)\\]\\s*\\|\\s*(.+)$/gm, '\\\\textbf{$1} \\\\hfill $2 \\\\\\\\');\n        setEditedResume(latex);\n    };\n\n    const profileUser = userResponse?.user ? { ...userResponse.user, isFollowing: userResponse.isFollowing } : null;\n\n    // 2. Dependent Queries\n    const userId = profileUser?.id;\n\n    const { data: snippetsData } = useQuery({\n        queryKey: ['user-snippets', userId],\n        queryFn: () => usersAPI.getSnippets(userId),\n        enabled: !!userId\n    });\n    const snippets = snippetsData?.snippets || [];\n\n    const { data: followersData } = useQuery({\n        queryKey: ['user-followers', userId],\n        queryFn: () => usersAPI.getFollowers(userId),\n        enabled: !!userId\n    });\n    const followers = followersData?.followers || [];\n\n    const { data: followingData } = useQuery({\n        queryKey: ['user-following', userId],\n        queryFn: () => usersAPI.getFollowing(userId),\n        enabled: !!userId\n    });\n    const following = followingData?.following || [];\n\n    // 3. Mutation\n    const followMutation = useMutation({\n        mutationFn: () => profileUser.isFollowing ? usersAPI.unfollow(userId) : usersAPI.follow(userId),\n        onMutate: async () => {\n            await queryClient.cancelQueries({ queryKey: ['user', username] });\n            const previousResponse = queryClient.getQueryData(['user', username]);\n\n            queryClient.setQueryData(['user', username], (old: any) => ({\n                ...old,\n                isFollowing: !old.isFollowing,\n                user: {\n                    ...old.user,\n                    _count: {\n                        ...old.user._count,\n                        followers: old.isFollowing\n                            ? (old.user._count.followers || 0) - 1\n                            : (old.user._count.followers || 0) + 1\n                    }\n                }\n            }));\n\n            return { previousResponse };\n        },\n        onError: (_, __, context: any) => {\n            queryClient.setQueryData(['user', username], context.previousResponse);\n        },\n        onSettled: () => {\n            queryClient.invalidateQueries({ queryKey: ['user', username] });\n            queryClient.invalidateQueries({ queryKey: ['user-followers', userId] });\n        }\n    });\n\n    const updateProfileMutation = useMutation({\n        mutationFn: (data: any) => usersAPI.update(data),\n        onSuccess: (data) => {\n            queryClient.invalidateQueries({ queryKey: ['user', username] });\n            updateUser(data.user);\n            setIsEditingResume(false);\n            toast({ title: \"Nexus protocol updated\" });\n        }\n    });\n\n    const handleSaveSettings = async () => {\n        setIsLoadingSettings(true);\n        try {\n            const response = await usersAPI.update({\n                name,\n                username: editedUsername,\n                bio,\n                visibility,\n                image: profileImage,\n                githubUrl,\n                instagramUrl\n            });\n            updateUser(response.user);\n            toast({ title: \"Configuration synced successfully\" });\n        } catch (error) {\n            toast({ title: \"Sync failed\", variant: \"destructive\" });\n        } finally {\n            setIsLoadingSettings(false);\n        }\n    };\n\n    // 4. Activity/Dashboard Logic\n    useEffect(() => {\n        if (view === 'dashboard' && profileUser) {\n            const fetchDashboardData = async () => {\n                try {\n                    const data = await snippetsAPI.getAll();\n                    setDashboardSnippets(data.snippets.slice(0, 3));\n                } catch (e) { console.error(e); }\n            };\n            fetchDashboardData();\n        }\n    }, [view, profileUser]);\n\n    // 5. Saved Logic\n    const fetchSavedData = async () => {\n        try {\n            const data = await snippetsAPI.getSaved();\n            setSavedSnippets(data.snippets);\n        } catch (e) { console.error(e); }\n    };\n\n    useEffect(() => {\n        if (view === 'saved' && profileUser?.id) {\n            fetchSavedData();\n        }\n    }, [view, profileUser?.id]);\n\n    // 6. Settings Initialization\n    useEffect(() => {\n        if (currentUser && currentUser.id === profileUser?.id) {\n            setName(currentUser.name || \"\");\n            setEditedUsername(currentUser.username || \"\");\n            setBio(currentUser.bio || \"\");\n            setVisibility(currentUser.visibility || \"PUBLIC\");\n        }\n    }, [currentUser, profileUser]);\n\n    // Sync image and social links when profileUser changes\n    useEffect(() => {\n        if (profileUser) {\n            setProfileImage(profileUser.image || \"\");\n            setGithubUrl(profileUser.githubUrl || \"\");\n            setInstagramUrl(profileUser.instagramUrl || \"\");\n        }\n    }, [profileUser]);\n\n    const handleFollow = () => {\n        if (profileUser) followMutation.mutate();\n    };\n\n    if (userLoading && !profileUser) {\n        return (\n            <div className=\"flex items-center justify-center min-h-[50vh]\">\n                <div className=\"h-4 w-4 rounded-sm bg-primary animate-ping\" />\n            </div>\n        );\n    }\n\n    if (!profileUser) {\n        return (\n            <div className=\"flex flex-col items-center justify-center min-h-[50vh] space-y-4\">\n                <Users className=\"h-12 w-12 text-muted-foreground/30\" />\n                <h3 className=\"text-xl font-bold font-mono\">USER_NOT_FOUND</h3>\n                <Button onClick={() => window.history.back()} variant=\"outline\">RETURN_</Button>\n            </div>\n        );\n    }\n\n    const UserList = ({ users }: { users: any[] }) => (\n        <div className=\"grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-4\">\n            {users.map((u) => (\n                <Link to={`/profile/${u.username}`} key={u.id} className=\"block group\">\n                    <div className=\"flex items-center gap-4 p-4 rounded-lg bg-surface border border-border hover:border-primary/30 transition-all\">\n                        <Avatar className=\"h-10 w-10 border border-border rounded-lg\">\n                            <AvatarImage src={u.image} />\n                            <AvatarFallback className=\"rounded-lg text-xs\">{u.username[0].toUpperCase()}</AvatarFallback>\n                        </Avatar>\n                        <div>\n                            <h4 className=\"font-bold text-sm text-foreground group-hover:text-primary transition-colors font-mono\">{u.name || u.username}</h4>\n                            <p className=\"text-[10px] text-muted-foreground font-mono\">@{u.username}</p>\n                        </div>\n                    </div>\n                </Link>\n            ))}\n            {users.length === 0 && <div className=\"col-span-full text-center py-12 text-muted-foreground italic font-mono text-xs\">NO_DATA_AVAILABLE</div>}\n        </div>\n    );\n\n    return (\n        <div className=\"space-y-6 max-w-7xl mx-auto p-6 pb-20 fade-in\">\n            {/* 1. Identity Matrix (Header) */}\n            <header className=\"bg-surface border border-border rounded-xl overflow-hidden shadow-sm relative group\">\n                {/* Background Pattern */}\n                <div className=\"absolute inset-0 bg-[url('https://grainy-gradients.vercel.app/noise.svg')] opacity-10 mix-blend-overlay pointer-events-none\" />\n                <div className=\"absolute top-0 right-0 w-64 h-64 bg-primary/5 blur-[100px] rounded-full pointer-events-none\" />\n\n                <div className=\"p-6 md:p-8 flex flex-col md:flex-row items-start md:items-end gap-6 md:gap-8 relative z-10\">\n                    <div className=\"relative group/avatar\">\n                        <div className=\"absolute -inset-2 bg-primary/20 rounded-xl blur-xl group-hover/avatar:opacity-100 opacity-0 transition-opacity duration-500\" />\n                        <Avatar className=\"h-32 w-32 md:h-40 md:w-40 border-4 border-surface rounded-2xl shadow-2xl\">\n                            <AvatarImage src={profileUser.image} className=\"object-cover\" />\n                            <AvatarFallback className=\"text-4xl bg-surface-hover font-bold text-primary rounded-2xl\">{profileUser.username[0].toUpperCase()}</AvatarFallback>\n                        </Avatar>\n                        {profileUser.badges?.length > 0 && (\n                            <div className=\"absolute -bottom-2 -right-2 bg-primary text-black p-1.5 rounded-lg border-2 border-surface z-20\">\n                                <Zap className=\"h-4 w-4 fill-current\" />\n                            </div>\n                        )}\n                    </div>\n\n                    <div className=\"flex-1 space-y-4\">\n                        <div>\n                            <div className=\"flex items-center gap-3 mb-1\">\n                                <h1 className=\"text-3xl md:text-5xl font-bold font-headline tracking-tight text-foreground\">\n                                    {profileUser.name || profileUser.username}\n                                </h1>\n                                {profileUser.visibility === 'PUBLIC' && <div className=\"h-2 w-2 rounded-full bg-green-500 shadow-[0_0_8px_rgba(34,197,94,0.5)]\" title=\"Public Protocol\" />}\n                            </div>\n                            <div className=\"flex flex-wrap items-center gap-4 text-sm font-mono text-muted-foreground\">\n                                <span className=\"text-primary\">@{profileUser.username}</span>\n                                <span className=\"text-border\">|</span>\n                                <span className=\"flex items-center gap-1.5\">\n                                    <MapPin className=\"h-3.5 w-3.5\" /> Earth\n                                </span>\n                                <span className=\"text-border\">|</span>\n                                <span className=\"flex items-center gap-1.5\">\n                                    <Calendar className=\"h-3.5 w-3.5\" /> Joined {format(new Date(profileUser.createdAt), 'MMM yyyy')}\n                                </span>\n                            </div>\n                        </div>\n\n                        {profileUser.bio && (\n                            <p className=\"text-sm text-foreground/80 max-w-2xl leading-relaxed border-l-2 border-primary/20 pl-4 py-1 italic\">\n                                \"{profileUser.bio}\"\n                            </p>\n                        )}\n\n                        <div className=\"flex flex-wrap gap-3 pt-2\">\n                            {/* Actions */}\n                            {currentUser?.id === profileUser.id ? (\n                                <Link to=\"/settings\">\n                                    <Button variant=\"outline\" size=\"sm\" className=\"h-9 px-6 text-xs font-mono uppercase tracking-wider border-border bg-surface hover:bg-surface-hover\">\n                                        <Settings className=\"h-3.5 w-3.5 mr-2\" /> Configure\n                                    </Button>\n                                </Link>\n                            ) : (\n                                <Button\n                                    onClick={handleFollow}\n                                    variant={profileUser?.isFollowing ? \"secondary\" : \"default\"}\n                                    size=\"sm\"\n                                    className=\"h-9 px-6 text-xs font-mono uppercase tracking-wider\"\n                                >\n                                    {profileUser?.isFollowing ? (\n                                        <>\n                                            <UserCheck className=\"h-3.5 w-3.5 mr-2\" /> Linked\n                                        </>\n                                    ) : (\n                                        <>\n                                            <Zap className=\"h-3.5 w-3.5 mr-2\" /> Connect\n                                        </>\n                                    )}\n                                </Button>\n                            )}\n\n                            {profileUser.githubUrl && (\n                                <a href={profileUser.githubUrl} target=\"_blank\" rel=\"noreferrer\">\n                                    <Button variant=\"ghost\" size=\"icon\" className=\"h-9 w-9 rounded-lg border border-border/50 text-muted-foreground hover:text-foreground\">\n                                        <Github className=\"h-4 w-4\" />\n                                    </Button>\n                                </a>\n                            )}\n                            {profileUser.instagramUrl && (\n                                <a href={profileUser.instagramUrl} target=\"_blank\" rel=\"noreferrer\">\n                                    <Button variant=\"ghost\" size=\"icon\" className=\"h-9 w-9 rounded-lg border border-border/50 text-muted-foreground hover:text-foreground\">\n                                        <Linkedin className=\"h-4 w-4\" />\n                                    </Button>\n                                </a>\n                            )}\n                        </div>\n                    </div>\n\n                    {/* Quick Stats (Vertical on mobile, right on desktop) */}\n                    <div className=\"flex md:flex-col gap-6 md:gap-2 text-right border-t md:border-t-0 md:border-l border-border pt-6 md:pt-0 md:pl-8 min-w-[120px]\">\n                        <div>\n                            <div className=\"text-2xl font-bold font-mono text-primary\">{profileUser._count.snippets}</div>\n                            <div className=\"text-[10px] uppercase tracking-widest text-muted-foreground font-mono\">Snippets</div>\n                        </div>\n                        <div>\n                            <div className=\"text-2xl font-bold font-mono text-foreground\">{profileUser._count.followers}</div>\n                            <div className=\"text-[10px] uppercase tracking-widest text-muted-foreground font-mono\">Connections</div>\n                        </div>\n                        <div>\n                            <div className=\"text-2xl font-bold font-mono text-muted-foreground\">{profileUser._count.following}</div>\n                            <div className=\"text-[10px] uppercase tracking-widest text-muted-foreground font-mono\">Linked</div>\n                        </div>\n                    </div>\n                </div>\n            </header>\n\n            {/* 2. Main Terminal Grid */}\n            <div className=\"grid grid-cols-1 lg:grid-cols-4 gap-6\">\n\n                {/* Left Column: Stats & Radar */}\n                <div className=\"space-y-6 lg:col-span-1\">\n                    <SkillRadar data={profileUser.skillPoints} />\n                    <BadgeGrid badges={profileUser.badges} profileUser={profileUser} />\n                </div>\n\n                {/* Right Column: Content Tabs */}\n                <div className=\"lg:col-span-3\">\n                    <Tabs value={view} onValueChange={setView} className=\"w-full\">\n                        <div className=\"flex items-center overflow-x-auto scrollbar-hide border-b border-border mb-6\">\n                            <TabsList className=\"bg-transparent h-12 p-0 gap-6\">\n                                {[\n                                    { id: 'snippets', label: 'Archives', icon: Code },\n                                    { id: 'dashboard', label: 'Command', icon: LayoutDashboard },\n                                    { id: 'saved', label: 'Vault', icon: Bookmark },\n                                    { id: 'resume', label: 'Manuscript', icon: FileText },\n                                    ...(currentUser?.id === profileUser.id ? [{ id: 'settings', label: 'Protocol', icon: Settings }] : []),\n                                    { id: 'followers', label: 'Vectors', icon: Users },\n                                    { id: 'following', label: 'Nexus', icon: UserCheck }\n                                ].map(tab => (\n                                    <TabsTrigger\n                                        key={tab.id}\n                                        value={tab.id}\n                                        className=\"bg-transparent border-b-2 border-transparent data-[state=active]:border-primary data-[state=active]:bg-transparent data-[state=active]:shadow-none rounded-none px-0 h-full font-mono text-xs uppercase tracking-widest text-muted-foreground data-[state=active]:text-primary hover:text-foreground transition-colors flex items-center gap-2\"\n                                    >\n                                        <tab.icon className=\"h-3.5 w-3.5\" />\n                                        {tab.label}\n                                    </TabsTrigger>\n                                ))}\n                            </TabsList>\n                        </div>\n\n                        <AnimatePresence mode=\"wait\">\n                            <motion.div\n                                key={view}\n                                initial={{ opacity: 0, y: 10 }}\n                                animate={{ opacity: 1, y: 0 }}\n                                exit={{ opacity: 0, y: -10 }}\n                                transition={{ duration: 0.2 }}\n                                className=\"min-h-[400px]\"\n                            >\n                                <TabsContent value=\"snippets\" className=\"mt-0 space-y-4\">\n                                    {profileUser.visibility === 'HYBRID' && currentUser?.id !== profileUser.id && (\n                                        <div className=\"bg-indigo-500/10 border border-indigo-500/20 p-3 rounded-lg flex items-center justify-center gap-2 text-indigo-400 text-xs font-mono uppercase tracking-wide\">\n                                            <Shield className=\"h-3.5 w-3.5\" /> Secure Protocol Active\n                                        </div>\n                                    )}\n\n                                    <div className=\"grid grid-cols-1 md:grid-cols-2 gap-4\">\n                                        {snippets\n                                            .filter((s: any) => {\n                                                if (profileUser.visibility === 'HYBRID' && currentUser?.id !== profileUser.id) {\n                                                    return profileUser.selectedPublicSnippetIds?.includes(s.id);\n                                                }\n                                                return true;\n                                            })\n                                            .map((snippet: any) => <SnippetCard key={snippet.id} snippet={snippet} />)\n                                        }\n                                        {snippets.length === 0 && (\n                                            <div className=\"col-span-full flex flex-col items-center justify-center py-20 text-muted-foreground border border-dashed border-border rounded-xl bg-canvas/30\">\n                                                <Terminal className=\"h-10 w-10 mb-4 opacity-20\" />\n                                                <p className=\"text-sm font-mono\">BUFFER_EMPTY</p>\n                                            </div>\n                                        )}\n                                    </div>\n                                </TabsContent>\n\n                                <TabsContent value=\"dashboard\" className=\"mt-0\">\n                                    <ProfileDashboard profileUser={profileUser} dashboardSnippets={dashboardSnippets} />\n                                </TabsContent>\n\n                                <TabsContent value=\"saved\" className=\"mt-0\">\n                                    <ProfileSaved savedSnippets={savedSnippets} onRefresh={fetchSavedData} />\n                                </TabsContent>\n\n                               