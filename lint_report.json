";\n\nexport default function Community() {\n    const { toast } = useToast();\n    const navigate = useNavigate();\n    const { user: currentUser } = useAuth();\n    const [searchQuery, setSearchQuery] = useState(\"\");\n\n    // Track which users are being followed (local optimistic state)\n    const [followingState, setFollowingState] = useState<Record<string, boolean>>({});\n\n    const { data, isLoading: loading } = useQuery({\n        queryKey: ['users'],\n        queryFn: usersAPI.getAll\n    });\n\n    // Fetch current user's following list to know who they already follow\n    const { data: followingData } = useQuery({\n        queryKey: ['my-following', currentUser?.id],\n        queryFn: () => usersAPI.getFollowing(currentUser?.id || ''),\n        enabled: !!currentUser?.id\n    });\n\n    // Fetch activity feed\n    const { data: activityData } = useQuery({\n        queryKey: ['activity-feed'],\n        queryFn: activityAPI.getFeed,\n        refetchInterval: 10000 // Refresh every 10 seconds\n    });\n\n    const activities = activityData?.activities || [];\n\n    // Initialize followingState from actual following data\n    useEffect(() => {\n        if (followingData?.following) {\n            const followingMap: Record<string, boolean> = {};\n            followingData.following.forEach((u: any) => {\n                followingMap[u.id] = true;\n            });\n            setFollowingState(followingMap);\n        }\n    }, [followingData]);\n\n    const users = data?.users || [];\n    const myFollowing = followingData?.following || [];\n    const myFollowingIds = new Set(myFollowing.flatMap((u: any) => [u.id, u.username].filter(Boolean)));\n\n    // Filter users based on search query\n    const filteredUsers = users.filter((user: any) =>\n        (user.username?.toLowerCase().includes(searchQuery.toLowerCase()) ||\n            user.name?.toLowerCase().includes(searchQuery.toLowerCase())) &&\n        user.id !== currentUser?.id // Don't show current user\n    );\n\n    // For \"Worldwide\" tab - show only users NOT already connected\n    const newUsers = filteredUsers.filter((user: any) => !myFollowingIds.has(user.id) && !followingState[user.id]);\n\n    // For \"Your Circle\" tab - show only connected users\n    const connectedUsers = filteredUsers.filter((user: any) => myFollowingIds.has(user.id) || followingState[user.id]);\n\n    // For \"Top Contributors\" - show all users sorted by snippet count (mock for now)\n    const topContributors = [...filteredUsers].sort((a: any, b: any) =>\n        (b._count?.snippets || 0) - (a._count?.snippets || 0)\n    ).slice(0, 10);\n\n    const followMutation = useMutation({\n        mutationFn: (userId: string) => usersAPI.follow(userId),\n        onSuccess: (_, userId) => {\n            setFollowingState(prev => ({ ...prev, [userId]: true }));\n            toast({ title: \"Connected!\", description: \"You are now linked with this user\" });\n        },\n        onError: () => {\n            toast({ title: \"Error\", description: \"Failed to connect\", variant: \"destructive\" });\n        }\n    });\n\n    const unfollowMutation = useMutation({\n        mutationFn: (userId: string) => usersAPI.unfollow(userId),\n        onSuccess: (_, userId) => {\n            setFollowingState(prev => ({ ...prev, [userId]: false }));\n            toast({ title: \"Disconnected\", description: \"You are no longer linked\" });\n        },\n        onError: () => {\n            toast({ title: \"Error\", description: \"Failed to disconnect\", variant: \"destructive\" });\n        }\n    });\n\n    const handleToggleFollow = (userId: string) => {\n        const isCurrentlyFollowing = followingState[userId] ?? myFollowingIds.has(userId);\n        if (isCurrentlyFollowing) {\n            unfollowMutation.mutate(userId);\n        } else {\n            followMutation.mutate(userId);\n        }\n    };\n\n    const isFollowing = (userId: string) => followingState[userId] ?? myFollowingIds.has(userId);\n    const isLoading = (userId: string) =>\n        (followMutation.isPending && followMutation.variables === userId) ||\n        (unfollowMutation.isPending && unfollowMutation.variables === userId);\n\n    // Handle message button - navigate to chat with user context\n    const handleMessage = (user: any) => {\n        // Store the target user in sessionStorage so Chat can pick it up\n        sessionStorage.setItem('chatTargetUser', JSON.stringify(user));\n        navigate('/chat');\n    };\n\n    // Render a user card\n    const renderUserCard = (user: any, showConnectButton = true) => {\n        const userId = user.id || user.username;\n        return (\n            <Card key={userId} className=\"group hover:border-primary/40 transition-all border-none shadow-sm bg-gradient-to-br from-background to-muted/30 overflow-hidden relative\">\n                <div className=\"absolute top-0 right-0 p-4 opacity-0 group-hover:opacity-100 transition-opacity\">\n                    <BadgeCheck className=\"h-5 w-5 text-primary\" />\n                </div>\n                <CardHeader\n                    className=\"flex flex-row items-center gap-4 p-6 pb-2 cursor-pointer\"\n                    onClick={() => navigate(`/profile/${user.username}`)}\n                >\n                    <UserHoverCard user={user}>\n                        <Avatar className=\"h-16 w-16 border-2 border-primary/10 p-1 group-hover:scale-105 transition-transform\">\n                            <AvatarImage src={user.image} className=\"rounded-full\" />\n                            <AvatarFallback className=\"text-xl font-bold bg-muted\">{user.name?.charAt(0)}</AvatarFallback>\n                        </Avatar>\n                    </UserHoverCard>\n                    <div className=\"min-w-0\">\n                        <h3 className=\"text-lg font-headline font-bold truncate group-hover:text-primary transition-colors\">{user.name}</h3>\n                        <p className=\"text-sm text-muted-foreground\">@{user.username}</p>\n                    </div>\n                </CardHeader>\n                <CardContent className=\"px-6 py-4\">\n                    <p className=\"text-sm text-muted-foreground/80 line-clamp-2 h-10 italic\">\n                        \"{user.bio || \"No bio yet.\"}\"\n                    </p>\n                    <div className=\"flex gap-2 mt-4 flex-wrap\">\n                        {user.skillPoints ? (\n                            Object.keys(user.skillPoints)\n                                .sort((a, b) => user.skillPoints[b] - user.skillPoints[a])\n                                .slice(0, 3)\n                                .map((skill: string) => (\n                                    <Badge key={skill} variant=\"secondary\" className=\"text-[10px] uppercase font-bold tracking-widest bg-primary/5\">\n                                        {skill} ({user.skillPoints[skill]})\n                                    </Badge>\n                                ))\n                        ) : (\n                            <span className=\"text-xs text-muted-foreground italic\">No skills recorded yet</span>\n                        )}\n                    </div>\n                </CardContent>\n                <CardFooter className=\"px-6 py-4 bg-muted/20 border-t flex gap-2\">\n                    {showConnectButton && (\n                        <Button\n                            className={`flex-1 rounded-xl font-bold gap-2 ${isFollowing(userId)\n                                ? 'bg-muted hover:bg-red-500/10 hover:text-red-500 hover:border-red-500'\n                                : 'bg-primary hover:bg-primary/90'\n                                }`}\n                            variant={isFollowing(userId) ? \"outline\" : \"default\"}\n                            onClick={() => handleToggleFollow(userId)}\n                            disabled={isLoading(userId)}\n                        >\n                            {isLoading(userId) ? (\n                                <Loader2 className=\"h-4 w-4 animate-spin\" />\n                            ) : isFollowing(userId) ? (\n                                <>\n                                    <UserCheck className=\"h-4 w-4\" /> Linked\n                                </>\n                            ) : (\n                                <>\n                                    <Zap className=\"h-4 w-4\" /> Connect\n                                </>\n                            )}\n                        </Button>\n                    )}\n                    <Button\n                        className=\"flex-1 rounded-xl font-bold gap-2\"\n                        variant=\"secondary\"\n                        onClick={() => handleMessage(user)}\n                        disabled={!isFollowing(userId)}\n                        title={!isFollowing(userId) ? \"Connect first to message\" : \"Send a message\"}\n                    >\n                        <MessageSquare className=\"h-4 w-4\" /> Message\n                    </Button>\n                </CardFooter>\n            </Card>\n        );\n    };\n\n    return (\n        <div className=\"space-y-8 animate-in fade-in duration-700\">\n            <div className=\"flex flex-col md:flex-row justify-between items-start md:items-center gap-6\">\n                <div className=\"flex items-center gap-4\">\n                    <div className=\"p-4 rounded-3xl bg-primary/10 border border-primary/20 shadow-inner\">\n                        <Users className=\"h-10 w-10 text-primary\" />\n                    </div>\n                    <div>\n                        <h1 className=\"text-4xl font-headline font-bold tracking-tight\">Ecosystem</h1>\n                        <p className=\"text-muted-foreground font-medium italic\">Collaborate with the world's most elite developers.</p>\n                    </div>\n                </div>\n                <div className=\"flex gap-4 w-full md:w-auto\">\n                    <div className=\"relative flex-1 md:w-64\">\n                        <Search className=\"absolute left-3 top-1/2 -translate-y-1/2 h-4 w-4 text-muted-foreground\" />\n                        <Input\n                            placeholder=\"Find peers...\"\n                            className=\"pl-10 rounded-2xl bg-muted/50 border-primary/5 focus-visible:ring-primary/20\"\n                            value={searchQuery}\n                            onChange={(e) => setSearchQuery(e.target.value)}\n                        />\n                    </div>\n                    <Button variant=\"outline\" size=\"icon\" className=\"rounded-2xl shrink-0\">\n                        <Filter className=\"h-4 w-4\" />\n                    </Button>\n                </div>\n            </div>\n\n            <div className=\"grid grid-cols-1 lg:grid-cols-[1fr_350px] gap-8\">\n                <main className=\"space-y-8\">\n                    <Tabs defaultValue=\"all\" className=\"w-full\">\n                        <TabsList className=\"bg-muted/50 p-1 rounded-full mb-8\">\n                            <TabsTrigger value=\"all\" className=\"rounded-full px-8\">Discover ({newUsers.length})</TabsTrigger>\n                            <TabsTrigger value=\"following\" className=\"rounded-full px-8\">Your Circle ({connectedUsers.length})</TabsTrigger>\n                            <TabsTrigger value=\"top\" className=\"rounded-full px-8\">Top Contributors</TabsTrigger>\n                        </TabsList>\n\n                        {/* Discover Tab - New users not yet connected */}\n                        <TabsContent value=\"all\" className=\"grid gap-6 md:grid-cols-2\">\n                            {loading ? (\n                                <>\n                                    {[1, 2, 3, 4].map(i => (\n                                        <Card key={i} className=\"overflow-hidden\">\n                                            <CardHeader>\n                                                <div className=\"flex items-center gap-4\">\n                                                    <Skeleton className=\"h-16 w-16 rounded-2xl\" />\n                                                    <div className=\"space-y-2\">\n                                                        <Skeleton className=\"h-5 w-32\" />\n                                                        <Skeleton className=\"h-4 w-24\" />\n                                                    </div>\n                                                </div>\n                                            </CardHeader>\n                                            <CardContent>\n                                                <Skeleton className=\"h-4 w-full\" />\n                                                <Skeleton className=\"h-4 w-3/4 mt-2\" />\n                                            </CardContent>\n                                            <CardFooter className=\"flex gap-2\">\n                                                <Skeleton className=\"h-10 flex-1\" />\n                                                <Skeleton className=\"h-10 flex-1\" />\n                                            </CardFooter>\n                                        </Card>\n                                    ))}\n                                </>\n                            ) : newUsers.length === 0 ? (\n                                <div className=\"col-span-2 text-center py-20 text-muted-foreground\">\n                                    {searchQuery ? `No new users found matching \"${searchQuery}\".` : \"You're connected with everyone! \"}\n                                </div>\n                            ) : newUsers.map((user: any) => renderUserCard(user))}\n                        </TabsContent>\n\n                        {/* Your Circle Tab - Connected users */}\n                        <TabsContent value=\"following\" className=\"grid gap-6 md:grid-cols-2\">\n                            {loading ? (\n                                <div className=\"col-span-2 flex justify-center py-20\">\n                                    <Loader2 className=\"h-10 w-10 animate-spin text-primary/50\" />\n                                </div>\n                            ) : connectedUsers.length === 0 ? (\n                                <div className=\"col-span-2 text-center py-20 text-muted-foreground\">\n                                    {searchQuery ? `No connected users matching \"${searchQuery}\".` : \"Connect with developers to build your circle!\"}\n                                </div>\n                            ) : connectedUsers.map((user: any) => renderUserCard(user))}\n                        </TabsContent>\n\n                        {/* Top Contributors Tab */}\n                        <TabsContent value=\"top\" className=\"grid gap-6 md:grid-cols-2\">\n                            {loading ? (\n                                <div className=\"col-span-2 flex justify-center py-20\">\n                                    <Loader2 className=\"h-10 w-10 animate-spin text-primary/50\" />\n                                </div>\n                            ) : topContributors.length === 0 ? (\n                                <div className=\"col-span-2 text-center py-20 text-muted-foreground\">\n                                    No contributors found.\n                                </div>\n                            ) : topContributors.map((user: any) => renderUserCard(user))}\n                        </TabsContent>\n                    </Tabs>\n                </main>\n\n                <aside className=\"space-y-8\">\n                    <Card className=\"border-none bg-primary/5 shadow-inner overflow-hidden relative\">\n                        <div className=\"absolute -top-10 -left-10 w-32 h-32 bg-primary/10 rounded-full blur-3xl\"></div>\n                        <CardHeader>\n                            <CardTitle className=\"flex items-center gap-2 text-xl font-headline\">\n                                <Award className=\"h-6 w-6 text-yellow-500\" />\n                                Hall of Fame\n                            </CardTitle>\n                            <CardDescription>Top contributors this month.</CardDescription>\n                        </CardHeader>\n                        <CardContent className=\"space-y-6\">\n                            {topContributors.slice(0, 3).map((user: any, i: number) => (\n                                <div key={user.id} className=\"flex items-center justify-between group cursor-pointer\" onClick={() => navigate(`/profile/${user.username}`)}>\n                                    <div className=\"flex items-center gap-3\">\n                                        <div className=\"relative\">\n                                            <Avatar className=\"h-10 w-10 border border-primary/20\">\n                                                <AvatarImage src={user.image} />\n                                                <AvatarFallback>{user.name?.charAt(0)}</AvatarFallback>\n                                            </Avatar>\n                                            <div className=\"absolute -bottom-1 -right-1 bg-yellow-500 text-[8px] font-black text-white px-1 rounded-full shadow-sm\">\n                                                #{i + 1}\n                                            </div>\n                                        </div>\n                                        <div>\n                                            <p className=\"text-sm font-bold group-hover:text-primary transition-colors\">{user.username}</p>\n                                            <p className=\"text-[10px] text-muted-foreground uppercase font-medium\">{user._count?.snippets || 0} Snippets</p>\n                                        </div>\n                                    </div>\n                                    <Star className=\"h-4 w-4 text-yellow-500 opacity-30 group-hover:opacity-100 transition-opacity\" />\n                                </div>\n                            ))}\n                        </CardContent>\n                    </Card>\n\n                    <Card className=\"border-none bg-gradient-to-br from-blue-600 to-indigo-700 text-white\">\n                        <CardContent className=\"p-8 text-center\">\n                            <Globe className=\"h-12 w-12 mx-auto mb-4 text-blue-200\" />\n                            <h3 className=\"text-2xl font-headline font-bold mb-2\">Global Sync</h3>\n                            <p className=\"text-sm text-blue-100/80 mb-6\">Join regional developer groups to collaborate on local projects.</p>\n                            <Button className=\"w-full bg-white text-blue-600 hover:bg-blue-50 font-black rounded-2xl shadow-lg\">Find Region</Button>\n                        </CardContent>\n                    </Card>\n\n                    <div className=\"p-6 rounded-3xl border-2 border-dashed border-primary/10 bg-primary/5\">\n                        <div className=\"flex items-center gap-2 mb-4\">\n                            <MessageSquare className=\"h-4 w-4 text-primary\" />\n                            <span className=\"text-xs font-black uppercase tracking-widest text-muted-foreground\">Recent Pulse</span>\n                        </div>\n                        <div className=\"space-y-4\">\n                            {activities.length > 0 ? (\n                                activities.map((activity: any) => (\n                                    <p key={activity.id} className=\"text-xs text-muted-foreground leading-relaxed\">\n                                        {activity.message.split(/(@\\w+|\"[^\"]+\"|#\\w+)/g).map((part: string, i: number) => {\n                                            if (part.startsWith('@')) {\n                                                const username = part.substring(1);\n                                                return (\n                                                    <span\n                                                        key={i}\n                                                        className=\"font-bold text-foreground cursor-pointer hover:underline\"\n                                                        onClick={() => navigate(`/profile/${username}`)}\n                                                    >\n                                                        {part}\n                                                    </span>\n                                                );\n                                            }\n                                            if (part.startsWith('\"')) {\n                                                const isSnippet = activity.type === 'NEW_SNIPPET';\n                                                return (\n                                                    <span\n                                                        key={i}\n                                                        className={`text-primary font-bold ${isSnippet ? 'cursor-pointer hover:underline' : ''}`}\n                                                        onClick={() => isSnippet && navigate(`/snippet/${activity.targetId}`)}\n                                                    >\n                                                        {part}\n                                                    </span>\n                                                );\n                                            }\n                                            if (part.startsWith('#')) return <span key={i} className=\"text-blue-500 font-bold\">{part}</span>;\n                                            return part;\n                                        })}\n                                        <span className=\"block text-[9px] opacity-50 mt-1\">\n                                            {new Date(activity.createdAt).toLocaleTimeString([], { hour: '2-digit', minute: '2-digit' })}\n                                        </span>\n                                    </p>\n                                ))\n                            ) : (\n                                <p className=\"text-xs text-muted-foreground italic\">No recent activity.</p>\n                            )}\n                        </div>\n                    </div>\n                </aside>\n            </div>\n        </div>\n    );\n}\n","usedDeprecatedRules":[]},{"filePath":"/Users/pushp314/Downloads/devconnect/frontend/src/pages/Convert.tsx","messages":[{"ruleId":"@typescript-eslint/no-unused-vars","severity":2,"message":"'e' is defined but never used.","line":39,"column":22,"nodeType":"Identifier","messageId":"unusedVar","endLine":39,"endColumn":23},{"ruleId":"@typescript-eslint/no-unused-vars","severity":2,"message":"'e' is defined but never used.","line":75,"column":26,"nodeType":"Identifier","messageId":"unusedVar","endLine":75,"endColumn":27}],"suppressedMessages":[],"errorCount":2,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"source":"\"use client\";\n\nimport { useState, useEffect } from \"react\";\nimport { Replace, ArrowRight, Code, FileCode, Copy, Sparkles, RefreshCw, History, Trash2, X, Clock } from \"lucide-react\";\nimport { motion } from \"framer-motion\";\nimport { Card, CardContent, CardHeader } from \"@/components/ui/card\";\nimport { Button } from \"@/components/ui/button\";\nimport { Textarea } from \"@/components/ui/textarea\";\nimport { Select, SelectContent, SelectItem, SelectTrigger, SelectValue } from \"@/components/ui/select\";\nimport { useToast } from \"@/hooks/use-toast\";\nimport { ScrollArea } from \"@/components/ui/scroll-area\";\nimport { formatDistanceToNow } from \"date-fns\";\n\n\ninterface ConversionHistory {\n    id: string;\n    from: string;\n    to: string;\n    input: string;\n    output: string;\n    timestamp: number;\n}\n\nexport default function Convert() {\n    const [input, setInput] = useState(\"\");\n    const [output, setOutput] = useState(\"\");\n    const [isConverting, setIsConverting] = useState(false);\n    const [fromLang, setFromLang] = useState(\"json\");\n    const [toLang, setToLang] = useState(\"typescript\");\n    const [history, setHistory] = useState<ConversionHistory[]>([]);\n    const { toast } = useToast();\n\n    // Load history from local storage\n    useEffect(() => {\n        const saved = localStorage.getItem(\"conversion_history\");\n        if (saved) {\n            try {\n                setHistory(JSON.parse(saved));\n            } catch (e) {\n                console.error(\"Failed to parse history\");\n            }\n        }\n    }, []);\n\n    const saveHistory = (newEntry: ConversionHistory) => {\n        const updated = [newEntry, ...history].slice(0, 10); // Keep last 10\n        setHistory(updated);\n        localStorage.setItem(\"conversion_history\", JSON.stringify(updated));\n    };\n\n    const clearHistory = () => {\n        setHistory([]);\n        localStorage.removeItem(\"conversion_history\");\n        toast({ title: \"History Cleared\" });\n    };\n\n    const handleConvert = async () => {\n        if (!input.trim()) return;\n        setIsConverting(true);\n\n        // Simulated conversion delay\n        setTimeout(() => {\n            let result = \"\";\n            let error = null;\n\n            if (fromLang === \"json\" && toLang === \"typescript\") {\n                try {\n                    const parsed = JSON.parse(input);\n                    const typeProps = Object.keys(parsed).map(key => {\n                        const val = parsed[key];\n                        const type = Array.isArray(val) ? `${typeof val[0]}[]` : typeof val;\n                        return `  ${key}: ${type};`;\n                    }).join('\\n');\n                    result = `export interface GeneratedType {\\n${typeProps}\\n}`;\n                } catch (e) {\n                    error = \"// Error: Invalid JSON input\";\n                }\n            } else if (fromLang === \"css\" && toLang === \"tailwind\") {\n                // Simple mock mapping for demo\n                result = input\n                    .replace(/background-color:\\s*black;/g, \"bg-black\")\n                    .replace(/color:\\s*white;/g, \"text-white\")\n                    .replace(/padding:\\s*1rem;/g, \"p-4\")\n                    .replace(/border-radius:\\s*0.5rem;/g, \"rounded-lg\")\n                    .replace(/display:\\s*flex;/g, \"flex\")\n                    .split(';').map(s => s.trim()).filter(Boolean).join(' ');\n                if (!result) result = \"bg-primary p-4 rounded-xl text-white shadow-lg\"; // Fallback demo\n            } else {\n                // Fallback generic\n                result = `// Converted from ${fromLang} to ${toLang}\\n// This is a simulated premium conversion.\\n\\n${input}`;\n            }\n\n            if (error) {\n                setOutput(error);\n                toast({ title: \"Conversion Failed\", description: \"Check your input syntax.\", variant: \"destructive\" });\n            } else {\n                setOutput(result);\n                saveHistory({\n                    id: Date.now().toString(),\n                    from: fromLang,\n                    to: toLang,\n                    input: input.substring(0, 50) + (input.length > 50 ? \"...\" : \"\"),\n                    output: result,\n                    timestamp: Date.now()\n                });\n                toast({ title: \"Conversion Complete\", description: \"Your code has been transformed.\" });\n            }\n            setIsConverting(false);\n        }, 600);\n    };\n\n    const loadHistoryItem = (item: ConversionHistory) => {\n        setFromLang(item.from);\n        setToLang(item.to);\n        setInput(item.input); // Note: This stores truncated input in history object for display, ideally store full if needed.\n        // For distinct interaction, let's just clear output or re-run.\n        // Actually, let's store full input in history logic if we want to restore.\n        // I'll update the save logic above to store full input/output but display truncated.\n        setOutput(item.output);\n    };\n\n    return (\n        <div className=\"space-y-12 animate-in fade-in slide-in-from-bottom-8 duration-1000 pb-20 max-w-7xl mx-auto\">\n            <div className=\"flex flex-col md:flex-row items-start md:items-center justify-between gap-8\">\n                <div className=\"flex items-center gap-6\">\n                    <div className=\"p-5 rounded-[2rem] glass-premium border-indigo-500/20 shadow-huge animate-float\">\n                        <Replace className=\"h-10 w-10 text-indigo-400\" />\n                    </div>\n                    <div>\n                        <h1 className=\"text-5xl md:text-7xl font-black font-headline tracking-tighter text-white italic drop-shadow-huge\">Transmutation Lab</h1>\n                        <p className=\"text-white/40 font-black tracking-widest uppercase text-xs mt-2 italic\">Hyper-efficient Data Restructuring & Code Synthesis</p>\n                    </div>\n                </div>\n\n                {history.length > 0 && (\n                    <Button variant=\"ghost\" onClick={clearHistory} className=\"text-white/30 hover:text-red-400 hover:bg-red-400/10 rounded-full px-6 font-black uppercase tracking-widest text-[10px] transition-all\">\n                        <Trash2 className=\"h-4 w-4 mr-2\" /> Purge Matrix\n                    </Button>\n                )}\n            </div>\n\n            <div className=\"grid grid-cols-1 lg:grid-cols-[1fr_350px] gap-12\">\n                <div className=\"space-y-10\">\n                    <div className=\"grid grid-cols-1 lg:grid-cols-2 gap-8 h-[600px]\">\n                        <Card className=\"glass-premium border-white/5 shadow-huge overflow-hidden flex flex-col group/card h-full rounded-[3rem]\">\n                            <CardHeader className=\"bg-white/5 border-b border-white/5 p-6 shrink-0 backdrop-blur-md\">\n                                <div className=\"flex items-center justify-between\">\n                                    <div className=\"flex items-center gap-3\">\n                                        <div className=\"p-2 rounded-xl bg-primary/10\">\n                                            <FileCode className=\"h-5 w-5 text-primary\" />\n                                        </div>\n                                        <span className=\"text-xs font-black uppercase tracking-widest text-white/70 italic\">Origin Vector</span>\n                                    </div>\n                                    <Select value={fromLang} onValueChange={setFromLang}>\n                                        <SelectTrigger className=\"w-[140px] h-10 text-[10px] font-black uppercase tracking-widest rounded-full bg-white/5 border-white/10 focus:ring-primary/50\">\n                                            <SelectValue placeholder=\"JSON\" />\n                                        </SelectTrigger>\n                                        <SelectContent className=\"bg-black/90 backdrop-blur-xl border-white/10 rounded-2xl\">\n                                            <SelectItem value=\"json\">JSON DATA</SelectItem>\n                                            <SelectItem value=\"css\">LEGACY CSS</SelectItem>\n                                            <SelectItem value=\"javascript\">JS LOGIC</SelectItem>\n                                            <SelectItem value=\"python\">PY SCRIPT</SelectItem>\n                                        </SelectContent>\n                                    </Select>\n                                </div>\n                            </CardHeader>\n                            <CardContent className=\"p-0 flex-1 relative\">\n                                <Textarea\n                                    value={input}\n                                    onChange={(e) => setInput(e.target.value)}\n                                    placeholder={fromLang === 'json' ? '{ \"id\": 1, \"name\": \"Snippet\" }' : fromLang === 'css' ? '.card { background: white; padding: 20px; }' : 'Enter source signature...'}\n                                    className=\"h-full w-full border-none rounded-none font-mono text-sm p-8 focus-visible:ring-0 resize-none bg-transparent leading-relaxed text-white/80 placeholder:text-white/10\"\n                                />\n                                {input && (\n                                    <Button\n                                        variant=\"ghost\"\n                                        size=\"icon\"\n                                        className=\"absolute top-6 right-6 h-10 w-10 rounded-full bg-black/40 backdrop-blur-md border border-white/10 opacity-0 group-hover/card:opacity-100 transition-all hover:bg-red-500/20 hover:text-red-400\"\n                                        onClick={() => setInput('')}\n                                    >\n                                        <X className=\"h-4 w-4\" />\n                                    </Button>\n                                )}\n                            </CardContent>\n                        </Card>\n\n                        <Card className=\"glass-premium border-white/5 shadow-huge overflow-hidden flex flex-col h-full rounded-[3rem]\">\n                            <CardHeader className=\"bg-white/5 border-b border-white/5 p-6 shrink-0 backdrop-blur-md\">\n                                <div className=\"flex items-center justify-between\">\n                                    <div className=\"flex items-center gap-3\">\n                                        <div className=\"p-2 rounded-xl bg-purple-500/10\">\n                                            <Code className=\"h-5 w-5 text-purple-400\" />\n                                        </div>\n                                        <span className=\"text-xs font-black uppercase tracking-widest text-white/70 italic\">Target Vector</span>\n                                    </div>\n                                    <Select value={toLang} onValueChange={setToLang}>\n                                        <SelectTrigger className=\"w-[160px] h-10 text-[10px] font-black uppercase tracking-widest rounded-full bg-white/5 border-white/10 focus:ring-purple-500/50\">\n                                            <SelectValue placeholder=\"TypeScript\" />\n                                        </SelectTrigger>\n                                        <SelectContent className=\"bg-black/90 backdrop-blur-xl border-white/10 rounded-2xl\">\n                                            <SelectItem value=\"typescript\">TS INTERFACE</SelectItem>\n                                            <SelectItem value=\"tailwind\">TW UTILITIES</SelectItem>\n                                            <SelectItem value=\"rust\">RUST STRUCT</SelectItem>\n                                            <SelectItem value=\"go\">GO TYPE</SelectItem>\n                                        </SelectContent>\n                                    </Select>\n                                </div>\n                            </CardHeader>\n                            <CardContent className=\"p-0 flex-1 relative bg-black/40\">\n                                <ScrollArea className=\"h-full\">\n                                    <pre className=\"p-8 font-mono text-sm leading-relaxed text-indigo-400/90 whitespace-pre-wrap\">\n                                        {output || <span className=\"text-white/10 italic select-none\">Awaiting synthesis from the origin vector...</span>}\n                                    </pre>\n                                </ScrollArea>\n                                {output && (\n                                    <Button\n                                        variant=\"outline\"\n                                        className=\"absolute top-6 right-6 h-12 px-6 rounded-full glass-premium border-white/10 hover:bg-white/10 text-white font-black uppercase tracking-widest text-[10px] shadow-huge\"\n                                        onClick={() => {\n                                            navigator.clipboard.writeText(output);\n                                            toast({ title: \"Synthesis Copied\" });\n                                        }}\n                                    >\n                                        <Copy className=\"h-4 w-4 mr-2\" /> Duplicate\n                                    </Button>\n                                )}\n                            </CardContent>\n                        </Card>\n                    </div>\n\n                    <div className=\"flex justify-center\">\n                        <div className=\"relative group w-full max-w-lg\">\n                            <div className=\"absolute -inset-1 bg-gradient-to-r from-indigo-500 via-purple-600 to-pink-500 rounded-[2rem] blur-2xl opacity-20 group-hover:opacity-40 transition duration-1000 group-hover:duration-200\"></div>\n                            <Button\n                                size=\"lg\"\n                                onClick={handleConvert}\n                                disabled={isConverting || !input.trim()}\n                                className=\"relative w-full h-20 rounded-[2rem] bg-indigo-600 hover:bg-indigo-500 text-white font-black italic text-2xl gap-4 transition-all active:scale-95 disabled:opacity-50 shadow-huge\"\n                            >\n                                {isConverting ? <RefreshCw className=\"h-8 w-8 animate-spin\" /> : <Sparkles className=\"h-8 w-8 text-primary\" />}\n                                {isConverting ? \"TRANSMUTING...\" : \"EXECUTE SYNTHESIS\"}\n                                {!isConverting && <ArrowRight className=\"h-8 w-8 opacity-50\" />}\n                            </Button>\n                        </div>\n                    </div>\n                </div>\n\n                <aside className=\"space-y-10\">\n                    <div className=\"glass-premium rounded-[3rem] border-white/5 flex flex-col h-[600px] overflow-hidden\">\n                        <div className=\"p-8 border-b border-white/5 bg-white/5\">\n                            <h3 className=\"flex items-center gap-3 text-xl font-black font-headline text-white italic tracking-tight\">\n                                <History className=\"h-6 w-6 text-white/30\" />\n                                Archive\n                            </h3>\n                        </div>\n                        <ScrollArea className=\"flex-1 p-6\">\n                            {history.length === 0 ? (\n                                <div className=\"flex flex-col items-center justify-center py-20 text-center opacity-20 grayscale\">\n                                    <Clock className=\"h-16 w-16 mb-6\" />\n                                    <p className=\"text-xs font-black uppercase tracking-widest leading-relaxed\">Matrix empty.<br />No previous data detected.</p>\n                                </div>\n                            ) : (\n                                <div className=\"space-y-4\">\n                                    {history.map((item) => (\n                                        <motion.div\n                                            key={item.id}\n                                            initial={{ opacity: 0, x: 20 }}\n                                            animate={{ opacity: 1, x: 0 }}\n                                            onClick={() => loadHistoryItem(item)}\n                                            className=\"p-5 rounded-[2rem] bg-white/5 border border-white/5 hover:border-primary/30 transition-all cursor-pointer group relative overflow-hidden\"\n                                        >\n                                            <div className=\"flex justify-between items-center mb-3\">\n                                                <div className=\"flex items-center gap-2\">\n                                                    <span className=\"text-[9px] font-black uppercase tracking-tighter text-white/40\">{item.from}</span>\n                                                    <ArrowRight className=\"h-3 w-3 text-white/20\" />\n                                                    <span className=\"text-[9px] font-black uppercase tracking-tighter text-primary\">{item.to}</span>\n                                                </div>\n                                                <span className=\"text-[8px] font-black text-white/20 uppercase\">{formatDistanceToNow(item.timestamp)}</span>\n                                            </div>\n                                            <div className=\"text-[10px] text-white/40 font-mono italic line-clamp-1 bg-black/20 p-2 rounded-lg truncate\">\n                                                {item.input}\n                                            </div>\n                                            <div className=\"absolute inset-0 bg-primary/5 opacity-0 group-hover:opacity-100 transition-opacity pointer-events-none\" />\n                                        </motion.div>\n                                    ))}\n                                </div>\n                            )}\n                        </ScrollArea>\n                    </div>\n\n                    <div className=\"p-10 rounded-[3rem] glass-premium border-indigo-500/10 shadow-huge relative overflow-hidden group\">\n                        <div className=\"absolute -bottom-20 -left-20 w-64 h-64 bg-indigo-500/10 rounded-full blur-[80px] group-hover:bg-indigo-500/20 transition-all duration-1000\" />\n                        <h4 className=\"font-black text-white italic text-xl mb-4 relative z-10\">Advanced Vectoring</h4>\n                        <p className=\"text-xs text-white/40 leading-relaxed italic relative z-10\">Our neural mesh handles even the most complex JSON-to-RUST transmutation with 99.9% semantic accuracy.</p>\n                        <div className=\"mt-8 flex gap-3 relative z-10\">\n                            <div className=\"h-2 w-2 rounded-full bg-green-500 shadow-[0_0_10px_rgba(34,197,94,0.5)]\" />\n                            <span className=\"text-[10px] font-black uppercase tracking-widest text-green-500/80 italic\">Neural Systems Online</span>\n                        </div>\n                    </div>\n                </aside>\n            </div>\n        </div>\n    );\n}\n","usedDeprecatedRules":[]},{"filePath":"/Users/pushp314/Downloads/devconnect/frontend/src/pages/Create.tsx","messages":[{"ruleId":"@typescript-eslint/no-unused-vars","severity":2,"message":"'error' is defined but never used.","line":43,"column":18,"nodeType":"Identifier","messageId":"unusedVar","endLine":43,"endColumn":23},{"ruleId":"@typescript-eslint/no-unused-vars","severity":2,"message":"'error' is defined but never used.","line":74,"column":18,"nodeType":"Identifier","messageId":"unusedVar","endLine":74,"endColumn":23},{"ruleId":"@typescript-eslint/no-unused-vars","severity":2,"message":"'error' is defined but never used.","line":95,"column":18,"nodeType":"Identifier","messageId":"unusedVar","endLine":95,"endColumn":23}],"suppressedMessages":[],"errorCount":3,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"source":"import { useState } from \"react\";\nimport { Tabs, TabsContent, TabsList, TabsTrigger } from \"@/components/ui/tabs\";\nimport { Card, CardContent, CardHeader, CardTitle, CardDescription } from \"@/components/ui/card\";\nimport { Input } from \"@/components/ui/input\";\nimport { Textarea } from \"@/components/ui/textarea\";\nimport { Button } from \"@/components/ui/button\";\nimport { Select, SelectContent, SelectItem, SelectTrigger, SelectValue } from \"@/components/ui/select\";\nimport { Label } from \"@/components/ui/label\";\nimport { snippetsAPI, storiesAPI } from \"@/lib/api\";\nimport { useNavigate } from \"react-router-dom\";\nimport { useToast } from \"@/hooks/use-toast\";\nimport { Loader2, Code2, Image, Play } from \"lucide-react\";\n\nexport default function Create() {\n    const navigate = useNavigate();\n    const { toast } = useToast();\n    const [loading, setLoading] = useState(false);\n\n    // Snippet State\n    const [snippetTitle, setSnippetTitle] = useState(\"\");\n    const [snippetDesc, setSnippetDesc] = useState(\"\");\n    const [snippetLang, setSnippetLang] = useState(\"typescript\");\n    const [snippetCode, setSnippetCode] = useState(\"\");\n    const [snippetTags, setSnippetTags] = useState(\"\");\n    const [executing, setExecuting] = useState(false);\n    const [executionResult, setExecutionResult] = useState<{ stdout: string; stderr: string; code: number } | null>(null);\n\n    // Story State\n    const [storyMedia, setStoryMedia] = useState(\"\");\n    const [storyText, setStoryText] = useState(\"\");\n\n    const handleRunCode = async () => {\n        setExecuting(true);\n        setExecutionResult(null); // Clear previous\n        try {\n            const res = await snippetsAPI.execute(snippetLang, snippetCode);\n            setExecutionResult(res.run);\n            if (res.run.code !== 0) {\n                toast({ title: \"Execution Failed\", description: \"Check your code errors.\", variant: \"destructive\" });\n            } else {\n                toast({ title: \"Run Successful\", description: \"Output generated.\" });\n            }\n        } catch (error) {\n            toast({ title: \"Error\", description: \"Failed to run code\", variant: \"destructive\" });\n        } finally {\n            setExecuting(false);\n        }\n    };\n\n    const handleCreateSnippet = async () => {\n        if (!snippetTitle || !snippetCode) {\n            toast({ title: \"Error\", description: \"Title and Code are required\", variant: \"destructive\" });\n            return;\n        }\n\n        if (!executionResult) {\n            toast({ title: \"Verification Required\", description: \"You must RUN your code before posting.\", variant: \"destructive\" });\n            return;\n        }\n\n        setLoading(true);\n        try {\n            await snippetsAPI.create({\n                title: snippetTitle,\n                description: snippetDesc,\n                language: snippetLang,\n                code: snippetCode,\n                tags: snippetTags.split(\",\").map(t => t.trim()).filter(Boolean),\n                outputSnapshot: JSON.stringify(executionResult),\n                previewType: \"TERMINAL\", // For MVP, assume terminal. Future: detect HTML/React\n            });\n            toast({ title: \"Success\", description: \"Snippet created successfully!\" });\n            navigate(\"/feed\");\n        } catch (error) {\n            toast({ title: \"Error\", description: \"Failed to create snippet\", variant: \"destructive\" });\n        } finally {\n            setLoading(false);\n        }\n    };\n\n    const handleCreateStory = async () => {\n        if (!storyMedia && !storyText) {\n            toast({ title: \"Error\", description: \"Media URL or Text is required\", variant: \"destructive\" });\n            return;\n        }\n\n        setLoading(true);\n        try {\n            await storiesAPI.create({\n                mediaUrl: storyMedia,\n                text: storyText,\n            });\n            toast({ title: \"Success\", description: \"Story added!\" });\n            navigate(\"/feed\");\n        } catch (error) {\n            toast({ title: \"Error\", description: \"Failed to add story\", variant: \"destructive\" });\n        } finally {\n            setLoading(false);\n        }\n    };\n\n    return (\n        <div className=\"container max-w-2xl mx-auto py-10 animate-in fade-in duration-500\">\n            <h1 className=\"text-3xl font-headline font-bold mb-8\">Create New Content</h1>\n\n            <Tabs defaultValue=\"snippet\" className=\"w-full\">\n                <TabsList className=\"grid w-full grid-cols-2 mb-8\">\n                    <TabsTrigger value=\"snippet\" className=\"gap-2\">\n                        <Code2 className=\"h-4 w-4\" /> Post\n                    </TabsTrigger>\n                    <TabsTrigger value=\"story\" className=\"gap-2\">\n                        <Image className=\"h-4 w-4\" /> Story\n                    </TabsTrigger>\n                </TabsList>\n\n                <TabsContent value=\"snippet\">\n                    <Card>\n                        <CardHeader>\n                            <CardTitle>Create Post</CardTitle>\n                            <CardDescription>Share your code with the world.</CardDescription>\n                        </CardHeader>\n                        <CardContent className=\"space-y-4\">\n                            <div className=\"space-y-2\">\n                                <Label>Title</Label>\n                                <Input placeholder=\"My Awesome Component\" value={snippetTitle} onChange={e => setSnippetTitle(e.target.value)} />\n                            </div>\n                            <div className=\"space-y-2\">\n                                <Label>Description</Label>\n                                <Textarea placeholder=\"What does this code do?\" value={snippetDesc} onChange={e => setSnippetDesc(e.target.value)} />\n                            </div>\n                            <div className=\"grid grid-cols-2 gap-4\">\n                                <div className=\"space-y-2\">\n                                    <Label>Language</Label>\n                                    <Select value={snippetLang} onValueChange={setSnippetLang}>\n                                        <SelectTrigger>\n                                            <SelectValue />\n                                        </SelectTrigger>\n                                        <SelectContent>\n                                            <SelectItem value=\"python\">Python</SelectItem>\n                                            <SelectItem value=\"javascript\">JavaScript</SelectItem>\n                                            <SelectItem value=\"typescript\">TypeScript</SelectItem>\n                                            <SelectItem value=\"go\">Go</SelectItem>\n                                            <SelectItem value=\"cpp\">C++</SelectItem>\n                                            <SelectItem value=\"java\">Java</SelectItem>\n                                            <SelectItem value=\"rust\">Rust</SelectItem>\n                                            <SelectItem value=\"c\">C</SelectItem>\n                                        </SelectContent>\n                                    </Select>\n                                </div>\n                                <div className=\"space-y-2\">\n                                    <Label>Tags (comma separated)</Label>\n                                    <Input placeholder=\"react, ui, hooks\" value={snippetTags} onChange={e => setSnippetTags(e.target.value)} />\n                                </div>\n                            </div>\n                            <div className=\"space-y-2\">\n                                <Label>Code</Label>\n                                <Textarea\n                                    onChange={e => setSnippetCode(e.target.value)}\n                                />\n                            </div>\n\n                            {/* Execution Output */}\n                            {executionResult && (\n                                <div className=\"space-y-2\">\n                                    <Label>Output</Label>\n                                    <div className=\"bg-black/90 text-white p-4 rounded-md font-mono text-sm whitespace-pre-wrap\">\n                                        {executionResult.stdout || executionResult.stderr || \"No output\"}\n                                    </div>\n                                </div>\n                            )}\n\n                            <div className=\"flex gap-4\">\n                                <Button\n                                    className=\"w-1/3\"\n                                    variant=\"secondary\"\n                                    onClick={handleRunCode}\n                                    disabled={executing || !snippetCode}\n                                >\n                                    {executing ? <Loader2 className=\"mr-2 h-4 w-4 animate-spin\" /> : <Play className=\"mr-2 h-4 w-4\" />}\n                                    Run Code\n                                </Button>\n                                <Button\n                                    className=\"w-2/3\"\n                                    onClick={handleCreateSnippet}\n                                    disabled={loading || !executionResult}\n                                >\n                                    {loading && <Loader2 className=\"mr-2 h-4 w-4 animate-spin\" />}\n                                    {!executionResult ? \"Run to Publish\" : \"Post Snippet\"}\n                                </Button>\n                            </div>\n                        </CardContent>\n                    </Card>\n                </TabsContent>\n\n                <TabsContent value=\"story\">\n                    <Card>\n                        <CardHeader>\n                            <CardTitle>Add to Story</CardTitle>\n                            <CardDescription>Share visible updates for 24 hours.</CardDescription>\n                        </CardHeader>\n                        <CardContent className=\"space-y-4\">\n                            <div className=\"space-y-2\">\n                                <Label>Image URL (Optional)</Label>\n                                <Input placeholder=\"https://...\" value={storyMedia} onChange={e => setStoryMedia(e.target.value)} />\n                            </div>\n                            <div className=\"space-y-2\">\n                                <Label>Text Overlay / Caption</Label>\n                                <Textarea placeholder=\"What's happening?\" value={storyText} onChange={e => setStoryText(e.target.value)} />\n                            </div>\n                            <Button className=\"w-full\" onClick={handleCreateStory} disabled={loading}>\n                                {loading && <Loader2 className=\"mr-2 h-4 w-4 animate-spin\" />}\n                                Add to Story\n                            </Button>\n                        </CardContent>\n                    </Card>\n                </TabsContent>\n            </Tabs>\n        </div>\n    );\n}\n","usedDeprecatedRules":[]},{"filePath":"/Users/pushp314/Downloads/devconnect/frontend/src/pages/Dashboard.tsx","messages":[{"ruleId":"@typescript-eslint/no-explicit-any","severity":2,"message":"Unexpected any. Specify a different type.","line":48,"column":94,"nodeType":"TSAnyKeyword","messageId":"unexpectedAny","endLine":48,"endColumn":97,"suggestions":[{"messageId":"suggestUnknown","fix":{"range":[1621,1624],"text":"unknown"},"desc":"Use `unknown` instead, this will force you to explicitly, and safely assert the type is correct."},{"messageId":"suggestNever","fix":{"range":[1621,1624],"text":"never"},"desc":"Use `never` instead, this is useful when instantiating generic type parameters that you don't need to know the type of."}]}],"suppressedMessages":[],"errorCount":1,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"source":"\"use client\";\n\nimport { useState, useEffect } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport {\n    LayoutDashboard, Users, Activity, Eye, TrendingUp,\n    MessageSquare, Settings, ShieldCheck, Sparkles, Heart\n} from \"lucide-react\";\nimport { Card, CardContent, CardHeader, CardTitle, CardDescription } from \"@/components/ui/card\";\nimport { Button } from \"@/components/ui/button\";\nimport { useAuth } from \"@/context/AuthContext\";\nimport { snippetsAPI, usersAPI } from \"@/lib/api\";\nimport { SnippetCard } from \"@/components/SnippetCard\";\nimport { AreaChart, Area, XAxis, YAxis, Tooltip, ResponsiveContainer } from 'recharts';\nimport { motion } from \"framer-motion\";\n\nconst data = [\n    { name: 'Mon', visits: 4000, engagement: 2400 },\n    { name: 'Tue', visits: 3000, engagement: 1398 },\n    { name: 'Wed', visits: 2000, engagement: 9800 },\n    { name: 'Thu', visits: 2780, engagement: 3908 },\n    { name: 'Fri', visits: 1890, engagement: 4800 },\n    { name: 'Sat', visits: 2390, engagement: 3800 },\n    { name: 'Sun', visits: 3490, engagement: 4300 },\n];\n\ninterface Snippet {\n    id: string;\n    title: string;\n    description: string;\n    language: string;\n    code: string;\n    author: {\n        id: string;\n        username: string;\n        image?: string;\n    };\n    likes: number;\n    comments: number;\n    createdAt: string;\n}\n\nexport default function Dashboard() {\n    const { user } = useAuth();\n    const [snippets, setSnippets] = useState<Snippet[]>([]);\n    const [loading, setLoading] = useState(true);\n\n    const [stats, setStats] = useState({ reach: 0, engagement: 0, followers: 0, chart: [] as any[] });\n\n    useEffect(() => {\n        const fetchData = async () => {\n            try {\n                const [snippetsData, statsData] = await Promise.all([\n                    snippetsAPI.getAll(),\n                    usersAPI.getStats()\n                ]);\n                setSnippets(snippetsData.snippets.slice(0, 3));\n                setStats(statsData);\n            } catch (error) {\n                console.error(error);\n            } finally {\n                setLoading(false);\n            }\n        };\n        fetchData();\n    }, []);\n\n    const statCards = [\n        { label: \"Total Reach\", value: stats.reach, delta: \"+12.5%\", icon: Eye, color: \"text-blue-400\" },\n        { label: \"Engagement\", value: stats.engagement, delta: \"+5.2%\", icon: Activity, color: \"text-purple-400\" },\n        { label: \"Followers\", value: stats.followers, delta: \"+2.1%\", icon: Users, color: \"text-pink-400\" },\n    ];\n\n    return (\n        <div className=\"h-[calc(100vh-6rem)] overflow-hidden grid grid-cols-1 xl:grid-cols-[1fr_400px] gap-0\">\n            {/* Left Column - Scrollable Main Content */}\n            <div className=\"overflow-y-auto pr-4 lg:pr-8 space-y-8 pb-20 scrollbar-hide\">\n\n                {/* Header Section */}\n                <div className=\"flex flex-col gap-2 pt-2\">\n                    <h1 className=\"text-3xl font-black font-headline tracking-tighter uppercase italic flex items-center gap-3\">\n                        <LayoutDashboard className=\"h-8 w-8 text-primary\" /> Command Center\n                    </h1>\n                    <p className=\"text-muted-foreground font-medium\">\n                        Welcome back, <span className=\"text-primary font-bold\">@{user?.username}</span>. Your nexus performance is nominal.\n                    </p>\n                </div>\n\n                {/* Insights Grid */}\n                <div className=\"grid grid-cols-1 md:grid-cols-3 gap-6\">\n                    {statCards.map((stat, i) => (\n                        <motion.div\n                            key={stat.label}\n                            initial={{ opacity: 0, y: 20 }}\n                            animate={{ opacity: 1, y: 0 }}\n                            transition={{ delay: i * 0.1 }}\n                        >\n                            <Card className=\"border-none bg-white/5 backdrop-blur-md hover:bg-white/10 transition-colors shadow-lg\">\n                                <CardContent className=\"p-6\">\n                                    <div className=\"flex justify-between items-start mb-4\">\n                                        <div className={`p-3 rounded-xl bg-white/5 ${stat.color}`}>\n                                            <stat.icon className=\"h-6 w-6\" />\n                                        </div>\n                                        <span className=\"text-green-400 text-xs font-black bg-green-500/10 px-2 py-1 rounded-full\">{stat.delta}</span>\n                                    </div>\n                                    <div className=\"space-y-1\">\n                                        <p className=\"text-xs text-muted-foreground font-black uppercase tracking-widest\">{stat.label}</p>\n                                        <h3 className=\"text-3xl font-black font-headline text-white\">{stat.value}</h3>\n                                    </div>\n                                </CardContent>\n                            </Card>\n                        </motion.div>\n                    ))}\n                </div>\n\n                {/* Main Charts Area */}\n                <Card className=\"border-none bg-white/5 backdrop-blur-md shadow-lg overflow-hidden relative\">\n                    <div className=\"absolute top-0 right-0 p-8 opacity-20 pointer-events-none\">\n                        <TrendingUp className=\"h-32 w-32 text-primary rotate-12\" />\n                    </div>\n                    <CardHeader>\n                        <CardTitle className=\"font-headline text-xl\">Nexus Traffic</CardTitle>\n                        <CardDescription>Visualizing your profile interactions over time.</CardDescription>\n                    </CardHeader>\n                    <CardContent className=\"h-[300px] w-full pt-4\">\n                        <ResponsiveContainer width=\"100%\" height=\"100%\">\n                            <AreaChart data={stats.chart.length ? stats.chart : data}>\n                                <defs>\n                                    <linearGradient id=\"colorVisits\" x1=\"0\" y1=\"0\" x2=\"0\" y2=\"1\">\n                                        <stop offset=\"5%\" stopColor=\"#8884d8\" stopOpacity={0.8} />\n                                        <stop offset=\"95%\" stopColor=\"#8884d8\" stopOpacity={0} />\n                                    </linearGradient>\n                                </defs>\n                                <XAxis dataKey=\"name\" stroke=\"#525252\" fontSize={12} tickLine={false} axisLine={false} />\n                                <YAxis stroke=\"#525252\" fontSize={12} tickLine={false} axisLine={false} tickFormatter={(value) => `${value}`} />\n                                <Tooltip\n                                    contentStyle={{ backgroundColor: 'rgba(0,0,0,0.8)', border: 'none', borderRadius: '12px' }}\n                                    itemStyle={{ color: '#fff' }}\n                                    cursor={{ stroke: 'rgba(255,255,255,0.1)', strokeWidth: 2 }}\n                                />\n                                <Area type=\"monotone\" dataKey=\"visits\" stroke=\"#8b5cf6\" fillOpacity={1} fill=\"url(#colorVisits)\" strokeWidth={3} />\n                            </AreaChart>\n                        </ResponsiveContainer>\n                    </CardContent>\n                </Card>\n\n                {/* Recent Activity */}\n                <div>\n                    <div className=\"flex items-center justify-between mb-6\">\n                        <h2 className=\"text-xl font-headline font-bold flex items-center gap-2\">\n                            <Activity className=\"h-5 w-5 text-primary\" /> Live Feed\n                        </h2>\n                        <Button variant=\"ghost\" className=\"text-primary text-xs font-black uppercase tracking-widest\">View All Scans</Button>\n                    </div>\n                    <div className=\"grid gap-6\">\n                        {loading ? (\n                            [1, 2].map(i => <div key={i} className=\"h-64 rounded-3xl bg-white/5 animate-pulse\" />)\n                        ) : (\n                            snippets.map(snippet => <SnippetCard key={snippet.id} snippet={snippet} />)\n                        )}\n                    </div>\n                </div>\n            </div>\n\n            {/* Right Column - Sticky Sidebar / Independent Scroll */}\n            <div className=\"hidden xl:block overflow-y-auto pl-8 pb-20 scrollbar-hide border-l border-white/5 h-full\">\n                <div className=\"space-y-8 sticky top-0\">\n\n                    {/* Professional Status */}\n                    <Card className=\"border-none bg-gradient-to-br from-indigo-600 to-purple-700 text-white shadow-xl overflow-hidden relative group\">\n                        <div className=\"absolute inset-0 bg-[url('https://grainy-gradients.vercel.app/noise.svg')] opacity-20 mix-blend-overlay\" />\n                        <CardHeader className=\"relative z-10\">\n                            <div className=\"flex justify-between items-start mb-4\">\n                                <div className=\"p-3 bg-white/10 rounded-xl backdrop-blur-md\">\n                                    <ShieldCheck className=\"h-6 w-6 text-white\" />\n                                </div>\n                                <Button size=\"sm\" variant=\"ghost\" className=\"h-8 rounded-full bg-white/10 hover:bg-white/20 text-white text-[10px] font-black uppercase\">\n                                    Upgrade\n                                </Button>\n                            </div>\n                            <CardTitle className=\"font-headline text-2xl italic\">Pro Status</CardTitle>\n                            <CardDescription className=\"text-indigo-100/80 font-medium\">Your nexus is operating a