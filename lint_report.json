rigger>\n                    <TabsTrigger value=\"tickets\">My Tickets</TabsTrigger>\n                    <TabsTrigger value=\"past\">Past</TabsTrigger>\n                </TabsList>\n\n                <TabsContent value=\"all\" className=\"mt-6 space-y-8\">\n                    {isLoading && (\n                        <div className=\"grid md:grid-cols-2 lg:grid-cols-3 gap-6\">\n                            {[1, 2, 3, 4, 5, 6].map(i => (\n                                <Card key={i} className=\"flex flex-col h-full\">\n                                    <CardHeader>\n                                        <div className=\"flex justify-between items-start mb-2\">\n                                            <Skeleton className=\"h-5 w-16\" />\n                                            <Skeleton className=\"h-5 w-12\" />\n                                        </div>\n                                        <Skeleton className=\"h-6 w-3/4\" />\n                                        <Skeleton className=\"h-4 w-full mt-2\" />\n                                    </CardHeader>\n                                    <CardContent className=\"flex-grow space-y-3\">\n                                        <Skeleton className=\"h-4 w-32\" />\n                                        <Skeleton className=\"h-4 w-40\" />\n                                    </CardContent>\n                                    <CardFooter>\n                                        <Skeleton className=\"h-10 w-full\" />\n                                    </CardFooter>\n                                </Card>\n                            ))}\n                        </div>\n                    )}\n\n                    {!isLoading && liveEvents.length > 0 && (\n                        <section>\n                            <h2 className=\"text-2xl font-semibold mb-4 flex items-center gap-2\">\n                                <span className=\"relative flex h-3 w-3\">\n                                    <span className=\"animate-ping absolute inline-flex h-full w-full rounded-full bg-red-400 opacity-75\"></span>\n                                    <span className=\"relative inline-flex rounded-full h-3 w-3 bg-red-500\"></span>\n                                </span>\n                                Live Now\n                            </h2>\n                            <div className=\"grid md:grid-cols-2 lg:grid-cols-3 gap-6\">\n                                {liveEvents.map(event => <EventCard key={event.id} event={event} />)}\n                            </div>\n                        </section>\n                    )}\n\n                    {!isLoading && upcomingEvents.length > 0 && (\n                        <section>\n                            <h2 className=\"text-2xl font-semibold mb-4\">Upcoming</h2>\n                            <div className=\"grid md:grid-cols-2 lg:grid-cols-3 gap-6\">\n                                {upcomingEvents.map(event => <EventCard key={event.id} event={event} />)}\n                            </div>\n                        </section>\n                    )}\n\n                    {!isLoading && pastEvents.length > 0 && (\n                        <section>\n                            <h2 className=\"text-2xl font-semibold mb-4\">Past Contests</h2>\n                            <div className=\"grid md:grid-cols-2 lg:grid-cols-3 gap-6\">\n                                {pastEvents.map(event => <EventCard key={event.id} event={event} />)}\n                            </div>\n                        </section>\n                    )}\n\n                    {!isLoading && filteredEvents.length === 0 && (\n                        <div className=\"text-center py-20 border-2 border-dashed border-border rounded-xl\">\n                            <Calendar className=\"h-10 w-10 mx-auto text-muted-foreground mb-4 opacity-50\" />\n                            <h3 className=\"text-lg font-semibold\">No Events Found</h3>\n                            <p className=\"text-muted-foreground\">Check back later for upcoming contests.</p>\n                        </div>\n                    )}\n                </TabsContent>\n\n                <TabsContent value=\"live\" className=\"mt-6\">\n                    <div className=\"grid md:grid-cols-2 lg:grid-cols-3 gap-6\">\n                        {liveEvents.map(event => <EventCard key={event.id} event={event} />)}\n                    </div>\n                </TabsContent>\n                <TabsContent value=\"upcoming\" className=\"mt-6\">\n                    <div className=\"grid md:grid-cols-2 lg:grid-cols-3 gap-6\">\n                        {upcomingEvents.map(event => <EventCard key={event.id} event={event} />)}\n                    </div>\n                </TabsContent>\n                <TabsContent value=\"past\" className=\"mt-6\">\n                    <div className=\"grid md:grid-cols-2 lg:grid-cols-3 gap-6\">\n                        {pastEvents.map(event => <EventCard key={event.id} event={event} />)}\n                    </div>\n                </TabsContent>\n\n                <TabsContent value=\"tickets\" className=\"mt-6\">\n                    <MyTicketsTab />\n                </TabsContent>\n            </Tabs>\n        </div>\n    );\n}\n\nfunction EventCard({ event }: { event: any }) {\n    const isLive = event.status === 'LIVE';\n    const isUpcoming = event.status === 'UPCOMING';\n\n    return (\n        <Card className=\"flex flex-col h-full hover:shadow-lg transition-shadow border-muted/50\">\n            <CardHeader>\n                <div className=\"flex justify-between items-start mb-2\">\n                    <Badge variant={isLive ? \"destructive\" : isUpcoming ? \"default\" : \"secondary\"}>\n                        {event.status}\n                    </Badge>\n                    {event.price > 0 ? (\n                        <Badge variant=\"outline\" className=\"text-green-500 border-green-500/50\">\n                            {event.price}\n                        </Badge>\n                    ) : (\n                        <Badge variant=\"outline\" className=\"text-blue-500 border-blue-500/50\">\n                            Free\n                        </Badge>\n                    )}\n                </div>\n                <CardTitle className=\"line-clamp-1\">{event.title}</CardTitle>\n                <CardDescription className=\"line-clamp-2\">{event.description}</CardDescription>\n            </CardHeader>\n            <CardContent className=\"flex-grow space-y-3 text-sm text-muted-foreground\">\n                <div className=\"flex items-center gap-2\">\n                    <Calendar className=\"h-4 w-4\" />\n                    <span>{format(new Date(event.startTime), \"MMM d, yyyy\")}</span>\n                </div>\n                <div className=\"flex items-center gap-2\">\n                    <Clock className=\"h-4 w-4\" />\n                    <span>{format(new Date(event.startTime), \"h:mm a\")} - {format(new Date(event.endTime), \"h:mm a\")}</span>\n                </div>\n                {isUpcoming && (\n                    <div className=\"text-xs font-mono bg-muted/50 p-2 rounded text-center\">\n                        <span className=\"text-muted-foreground mr-2\">Starts in</span>\n                        <CountdownTimer targetDate={new Date(event.startTime)} />\n                    </div>\n                )}\n            </CardContent>\n            <CardFooter>\n                <Button asChild className=\"w-full group\">\n                    <Link to={`/arena/events/${event.id}`}>\n                        {isLive ? \"Enter Contest\" : isUpcoming ? \"Register Now\" : \"View Details\"}\n                        <ArrowRight className=\"w-4 h-4 ml-2 group-hover:translate-x-1 transition-transform\" />\n                    </Link>\n                </Button>\n            </CardFooter>\n        </Card>\n    );\n}\n\nfunction MyTicketsTab() {\n    const { data, isLoading } = useQuery({\n        queryKey: ['my-registrations'],\n        queryFn: registrationsAPI.getMyRegistrations\n    });\n\n    const registrations = data?.registrations || [];\n\n    if (isLoading) {\n        return (\n            <div className=\"flex justify-center py-20\">\n                <Loader2 className=\"h-8 w-8 animate-spin text-primary/50\" />\n            </div>\n        );\n    }\n\n    if (registrations.length === 0) {\n        return (\n            <div className=\"text-center py-20 border-2 border-dashed border-border rounded-xl\">\n                <Ticket className=\"h-10 w-10 mx-auto text-muted-foreground mb-4 opacity-50\" />\n                <h3 className=\"text-lg font-semibold\">No Tickets Yet</h3>\n                <p className=\"text-muted-foreground\">Register for an event to see your ticket here.</p>\n            </div>\n        );\n    }\n\n    return (\n        <div className=\"grid md:grid-cols-2 lg:grid-cols-3 gap-6\">\n            {registrations.map((reg: any) => (\n                <div key={reg.id} className=\"relative group\">\n                    <div className=\"absolute top-2 right-2 z-10 w-auto\">\n                        <Badge className=\"bg-green-500 hover:bg-green-600 shadow-md\">Ticket: {reg.id.slice(0, 8)}</Badge>\n                    </div>\n                    {/* Reuse EventCard but pass the event object from registration */}\n                    <EventCard event={reg.event} />\n                </div>\n            ))}\n        </div>\n    );\n}\n","usedDeprecatedRules":[]},{"filePath":"/Users/pushp314/Downloads/devconnect/frontend/src/pages/arena/Environment.tsx","messages":[{"ruleId":"@typescript-eslint/no-explicit-any","severity":2,"message":"Unexpected any. Specify a different type.","line":27,"column":60,"nodeType":"TSAnyKeyword","messageId":"unexpectedAny","endLine":27,"endColumn":63,"suggestions":[{"messageId":"suggestUnknown","fix":{"range":[1370,1373],"text":"unknown"},"desc":"Use `unknown` instead, this will force you to explicitly, and safely assert the type is correct."},{"messageId":"suggestNever","fix":{"range":[1370,1373],"text":"never"},"desc":"Use `never` instead, this is useful when instantiating generic type parameters that you don't need to know the type of."}]},{"ruleId":"@typescript-eslint/no-unused-vars","severity":2,"message":"'e' is defined but never used.","line":68,"column":22,"nodeType":"Identifier","messageId":"unusedVar","endLine":68,"endColumn":23},{"ruleId":"react-hooks/exhaustive-deps","severity":1,"message":"React Hook useEffect has a missing dependency: 'code'. Either include it or remove the dependency array.","line":74,"column":8,"nodeType":"ArrayExpression","endLine":74,"endColumn":31,"suggestions":[{"desc":"Update the dependencies array to be: [problemData, language, code]","fix":{"range":[3119,3142],"text":"[problemData, language, code]"}}]},{"ruleId":"@typescript-eslint/no-explicit-any","severity":2,"message":"Unexpected any. Specify a different type.","line":109,"column":24,"nodeType":"TSAnyKeyword","messageId":"unexpectedAny","endLine":109,"endColumn":27,"suggestions":[{"messageId":"suggestUnknown","fix":{"range":[4444,4447],"text":"unknown"},"desc":"Use `unknown` instead, this will force you to explicitly, and safely assert the type is correct."},{"messageId":"suggestNever","fix":{"range":[4444,4447],"text":"never"},"desc":"Use `never` instead, this is useful when instantiating generic type parameters that you don't need to know the type of."}]},{"ruleId":"@typescript-eslint/no-unused-vars","severity":2,"message":"'e' is defined but never used.","line":148,"column":18,"nodeType":"Identifier","messageId":"unusedVar","endLine":148,"endColumn":19},{"ruleId":"@typescript-eslint/no-explicit-any","severity":2,"message":"Unexpected any. Specify a different type.","line":193,"column":62,"nodeType":"TSAnyKeyword","messageId":"unexpectedAny","endLine":193,"endColumn":65,"suggestions":[{"messageId":"suggestUnknown","fix":{"range":[849