imize2 className=\"h-4 w-4\" />\n                                </Button>\n                            </div>\n\n                            <div className=\"flex items-center justify-between pt-1 border-t border-white/5\">\n                                <SnippetInteraction\n                                    snippet={snippet}\n                                    className=\"border-none p-0 text-white/80 hover:text-white\"\n                                    onCommentClick={() => setShowComments(true)}\n                                    onShareClick={(e) => { e?.stopPropagation(); setShowShare(true); }}\n                                />\n                                <Button size=\"icon\" variant=\"ghost\" className=\"h-8 w-8 text-white/50 hover:text-white hover:bg-white/10 rounded-full\">\n                                    <Share2 className=\"h-4 w-4\" />\n                                </Button>\n                            </div>\n                        </div>\n                    </div>\n\n                </div>\n            </motion.div>\n\n            <SnippetDetailModal\n                snippet={snippet}\n                open={showDetail}\n                onOpenChange={setShowDetail}\n            />\n\n            <CommentSidebar\n                open={showComments}\n                onOpenChange={setShowComments}\n                snippetId={snippet.id}\n            />\n\n            <ShareDialog\n                open={showShare}\n                onOpenChange={setShowShare}\n                url={`${window.location.origin}/snippet/${snippet.id}`}\n                title={snippet.title}\n            />\n\n            {\n                isAuthor && (\n                    <EditSnippetModal\n                        snippet={snippet}\n                        open={showEdit}\n                        onOpenChange={setShowEdit}\n                    />\n                )\n            }\n        </>\n    );\n}\n","usedDeprecatedRules":[]},{"filePath":"/User